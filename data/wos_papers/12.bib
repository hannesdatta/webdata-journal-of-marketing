@article{ WOS:000165697600003,
Author = {Desai, KK and Hoyer, WD},
Title = {Descriptive characteristics of memory-based consideration sets:
   Influence of usage occasion frequency and usage location familiarity},
Journal = {JOURNAL OF CONSUMER RESEARCH},
