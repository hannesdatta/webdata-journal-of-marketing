@article{ WOS:000172762300002,
Author = {Raghunathan, R and Irwin, JR},
Title = {Walking the hedonic product treadmill: Default contrast and mood-based
   assimilation in judgments of predicted happiness with a target product},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2001},
Volume = {28},
Number = {3},
Pages = {355-368},
Month = {DEC},
Abstract = {Consumers often browse through many products (a product context) before
   evaluating a particular target product. We examine the influence of four
   product context characteristics on happiness with a target product:
   pleasantness, sequence, domain match with target (i.e., whether products
   in the context set belong to the same category as the target), and
   context set size. When context and target match, pleasant and improving
   (compared to less pleasant and worsening) contexts induce less happiness
   with the target product. When there is domain mismatch, however, the
   results are reversed. Furthermore, the assimilation effects are
   significantly influenced by set size, but the contrast effects are not.
   While the contrast effects appear to occur by default and appear to be
   driven by a process of comparison, the assimilation effects appear to be
   driven by mood. These effects hold even when perception of domain match
   is manipulated via instructional framing.},
