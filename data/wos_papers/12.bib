@article{ WOS:000172928300007,
Author = {Kumar, N and Rajiv, S and Jeuland, A},
Title = {Effectiveness of trade promotions: Analyzing the determinants of retail
   pass through},
Journal = {MARKETING SCIENCE},
Year = {2001},
