@article{ WOS:000172160300007,
Author = {Aaker, DA and Jacobson, R},
Title = {The value relevance of brand attitude in high-technology markets},
Journal = {JOURNAL OF MARKETING RESEARCH},
Year = {2001},
Volume = {38},
Number = {4},
Pages = {485-493},
Month = {NOV},
Abstract = {The authors assess the extent to which brand attitude, a key component
   of brand equity, has value relevance-that is, helps predict future
   earnings and thus firm value-in high-technology markets. Using data for
   firms in the computer industries, the authors find that changes in brand
   attitude are associated contemporaneously with stock return and lead
   accounting financial performance. The study also provides insights into
   the drivers of brand attitude. The authors find that both product
   attributes and peripheral cues shape brand attitudes in high-technology
   markets.},
