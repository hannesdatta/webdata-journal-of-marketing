@article{ WOS:000169280300003,
Author = {Janiszewski, C and Meyvis, T},
Title = {Effects of brand logo complexity, repetition, and spacing on processing
   fluency and judgment},
Journal = {JOURNAL OF CONSUMER RESEARCH},
