@article{ WOS:000172160300003,
Author = {Sandor, Z and Wedel, M},
Title = {Designing conjoint choice experiments using managers' prior beliefs},
Journal = {JOURNAL OF MARKETING RESEARCH},
Year = {2001},
Volume = {38},
Number = {4},
Pages = {430-444},
Month = {NOV},
Abstract = {The authors provide more efficient designs for conjoint choice
   experiments based on prior information elicited from managers about the
   parameters and their associated uncertainty. The authors use a Bayesian
   design procedure that assumes a prior distribution of likely parameter
   values and optimizes the design over that distribution. The authors
   propose a way to elicit prior information from managers and show in
   Monte Carlo studies that the procedure provides more efficient designs
   than the current procedures. The authors provide an empirical
   application, in which they elicit prior information on the parameter
   values and the associated uncertainty from managers. Here, the Bayesian
   design provides 30\%-50\% lower standard errors of the estimates and an
   expected predictive validity that is approximately 20\% higher.},
