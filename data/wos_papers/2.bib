@article{ WOS:000461770900004,
Author = {Jami, Ata},
Title = {Having Control Over and Above Situations: The Influence of Elevated
   Viewpoints on Risk Taking},
Journal = {JOURNAL OF MARKETING RESEARCH},
Year = {2019},
Volume = {56},
Number = {2},
Pages = {230-244},
Month = {APR},
Abstract = {This article examines how consumers' exposure to the viewpoint of high
   versus low vertical position changes their risk-taking behavior. The
   author proposes that consumers' views of scenery from a high physical
   elevation induce an illusory sense of control, which in turn intensifies
   risk taking. Multiple studies show that exposure to the viewpoint of
   high vertical positions increases risk taking in both laboratory
   settings (Studies 1a, 1b, and 1c) and real settings (Study 4). In
   addition, the author demonstrates that an illusory sense of control
   mediates the effect of an elevated viewpoint on risk taking (Study 2)
   and that the effect of elevation on risk taking is attenuated when
   people use a low-level mental construal to process visual information
   (Study 3).},
