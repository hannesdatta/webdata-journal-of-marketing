@article{ WOS:000429505200003,
Author = {Argo, Jennifer J. and Dahl, Darren W.},
Title = {Standards of Beauty: The Impact of Mannequins in the Retail Context},
Journal = {JOURNAL OF CONSUMER RESEARCH},
