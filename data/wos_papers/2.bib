@article{ WOS:000430160700006,
Author = {Bajaj, Aditi and Bond, Samuel D.},
Title = {Beyond Beauty: Design Symmetry and Brand Personality},
Journal = {JOURNAL OF CONSUMER PSYCHOLOGY},
