ISSN = {1057-7408},
EISSN = {1532-7663},
Keywords = {Behavioral decision theory; Charity and prosocial behavior; Decision
   making; Ethics and morality; Preference and choice},
Keywords-Plus = {PROSOCIAL BEHAVIOR; SELF-INTEREST; UNCERTAINTY; PREFERENCE; ALTRUISM;
