@article{ WOS:000425455100003,
Author = {Caldieraro, Fabio and Zhang, Jonathan Z. and Cunha, Jr., Marcus and
   Shulman, Jeffrey D.},
Title = {Strategic Information Transmission in Peer-to-Peer Lending Markets},
Journal = {JOURNAL OF MARKETING},
