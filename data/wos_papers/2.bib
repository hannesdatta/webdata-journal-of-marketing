@article{ WOS:000458113200003,
Author = {Molner, Sven and Prabhu, Jaideep C. and Yadav, Manjit S.},
Title = {Lost in a Universe of Markets: Toward a Theory of Market Scoping for
   Early-Stage Technologies},
Journal = {JOURNAL OF MARKETING},
Year = {2019},
Volume = {83},
Number = {2},
Pages = {37-61},
Month = {MAR},
Abstract = {This article examines market scoping for early-stage technologies, a
   fundamental yet underexplored marketing activity. Market scoping refers
   to managerial activities directed at the identification of market spaces
   for early-stage technologies. This discovery-oriented research aimed at
   theory development draws on an extensive, multiyear database of email
   trails and archival records detailing market-scoping efforts for
   early-stage technologies emerging from a global research university.
   From this longitudinal database, the authors provide an in-depth
   examination of managers' market space decisions and advance an initial
   theory of market scoping. They isolate managers' market-scoping
   mindse-which manifests as market ambiguity avoidance or acceptanceas-a
   key explanatory construct shaping market space decisions and outcomes.
   Market ambiguity avoidance results in managers' downstream orientation
   toward end users; this mindset, counterintuitively, may lead to
   technology commercialization failure. In contrast, market ambiguity
   acceptance results in managers' upstream orientation; this mindset
   directs attention away from end users but helps uncover indirect paths
   to viable market spaces. This article lays the groundwork for advancing
   marketing research in the context of early-stage technology
   commercialization.},
