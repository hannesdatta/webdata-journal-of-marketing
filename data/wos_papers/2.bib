@article{ WOS:000453375700007,
Author = {Gupta, Aditya and Kumar, Alok and Grewal, Rajdeep and Lilien, Gary L.},
Title = {Within-Seller and Buyer-Seller Network Structures and Key Account
   Profitability},
Journal = {JOURNAL OF MARKETING},
