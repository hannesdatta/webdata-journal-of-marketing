@article{ WOS:000446169400004,
Author = {Jiao, Jinfeng (Jenny) and Wang, Jing},
Title = {Can Lonely People Behave Morally? The Joint Influence of Loneliness and
   Empathy on Moral Identity},
Journal = {JOURNAL OF CONSUMER PSYCHOLOGY},
Year = {2018},
Volume = {28},
Number = {4},
Pages = {597-611},
Month = {OCT},
Abstract = {We examine how loneliness moderates the effect of empathy on moral
   identity: the extent to which being a moral person is important to an
   individual's identity. Results of four studies show that only lonely
   people have increased moral identity when they have high (vs. low)
   empathy; empathy does not reliably increase moral identity for nonlonely
   people. We demonstrate these effects with psychological measures of
   moral identity as well as with downstream moral behaviors in various
   consumer settings. Our findings are consistent with the motivation
   theory of loneliness and empathy: Lonely people are capable, but are not
   motivated to empathize.},
