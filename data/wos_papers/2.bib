@article{ WOS:000449361400008,
Author = {Li, Yang and Gordo, Brett R. and Netzer, Oded},
Title = {An Empirical Study of National vs. Local Pricing by Chain Stores Under
   Competition},
Journal = {MARKETING SCIENCE},
Year = {2018},
Volume = {37},
Number = {5},
Pages = {812-837},
Month = {SEP-OCT},
Abstract = {Geographic price discrimination is generally considered beneficial to
   firm profitability. However, theoretical results point to conditions
   under which firms might prefer to price across markets uniformly in
   oligopolistic settings. This paper provides an empirical analysis of
   competitive price discrimination and quantitatively assesses the
   profitability of national pricing relative to store-level pricing
   policies under different market conditions. Specifically, we construct
   and estimate a model of retail competition using extensive data from the
   digital camera market. A series of counterfactuals show that, under
   reasonable commitment mechanisms, two leading chains would benefit from
   employing national pricing policies, whereas a discount retailer should
   target prices in each local market. Additional results explore the
   boundary conditions of these findings and evaluate hybrid pricing
   policies.},
