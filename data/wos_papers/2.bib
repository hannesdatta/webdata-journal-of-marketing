@article{ WOS:000458113200007,
Author = {Lee, Hannah S. and Griffith, David A.},
Title = {Social Comparison in Retailer-Supplier Relationships: Referent
   Discrepancy Effects},
Journal = {JOURNAL OF MARKETING},
