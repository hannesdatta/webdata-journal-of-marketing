@article{ WOS:000454584400013,
Author = {Sanchez, Diana T. and Chaney, Kimberly E. and Maimon, Melanie R.},
Title = {Stigmatized-Identity Cues and Consumer Applications Revisited},
Journal = {JOURNAL OF CONSUMER PSYCHOLOGY},
Year = {2019},
Volume = {29},
