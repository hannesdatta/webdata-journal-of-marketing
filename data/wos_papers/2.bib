@article{ WOS:000423123200007,
Author = {Kireyev, Pavel and Kumar, Vineet and Ofek, Elie},
Title = {Match Your Own Price? Self-Matching as a Retailer's Multichannel Pricing
   Strategy},
Journal = {MARKETING SCIENCE},
Year = {2017},
Volume = {36},
