@article{ WOS:000449045700007,
Author = {Shen, Hao and Sengupta, Jaideep},
Title = {Word of Mouth versus Word of Mouse: Speaking about a Brand Connects You
   to It More Than Writing Does},
Journal = {JOURNAL OF CONSUMER RESEARCH},
