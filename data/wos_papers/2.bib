@article{ WOS:000427991000001,
Author = {Wang, Yang and Chaudhry, Alexander},
Title = {When and How Managers' Responses to Online Reviews Affect Subsequent
   Reviews},
Journal = {JOURNAL OF MARKETING RESEARCH},
