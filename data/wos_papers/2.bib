@article{ WOS:000427991000004,
Author = {Kumar, V. and Leszkiewicz, Agata and Herbst, Angeliki},
Title = {Are You Back for Good or Still Shopping Around? Investigating Customers'
   Repeat Churn Behavior},
Journal = {JOURNAL OF MARKETING RESEARCH},
