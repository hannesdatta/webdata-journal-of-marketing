@article{ WOS:000252244300006,
Author = {Li, Xiuping},
Title = {The effects of appetitive stimuli on out-of-domain consumption
   impatience},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2008},
Volume = {34},
Number = {5},
Pages = {649-656},
Month = {FEB},
Abstract = {Earlier work in consumer research has documented the effect of
   appetitive stimuli (e.g., chocolate cookies) on a related consumption
   domain (e.g., eating). We argue that appetitive stimuli can lead to a
   change in temporal orientation and affect subsequent consumption
   impatience across domains. In a series of experiments, we find that
   consumers exposed to appetitive stimuli are more present oriented, more
   likely to choose smaller-sooner rewards or vice options, and more likely
   to make unplanned purchase decisions.},
