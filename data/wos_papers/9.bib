@article{ WOS:000261400100003,
Author = {Wyer, Jr., Robert S. and Hung, Iris W. and Jiang, Yuwei},
Title = {Visual and verbal processing strategies in comprehension and judgment},
Journal = {JOURNAL OF CONSUMER PSYCHOLOGY},
Year = {2008},
Volume = {18},
Number = {4},
Pages = {244-257},
Abstract = {The information we receive in the course of daily life experience is
   often transmitted both verbally and visually. Two different processing
   strategies are postulated to underlie the integration of this
   information, the activation of which may be influenced by (a) chronic
   individual differences in the disposition to process information
   visually vs. verbally, (b) situational factors that influence the
   relative accessibility of these strategies in memory, and (c)
   characteristics of the information to be processed. Research in both
   social and consumer psychology is discussed in terms of the conceptual
   framework we propose. (C) 2008 Society for Consumer Psychology.
   Published by Elsevier Inc. All rights reserved.},
