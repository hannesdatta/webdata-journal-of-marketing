@article{ WOS:000242126600001,
Author = {Mishra, Arul and Nayakankuppam, Dhananjay},
Title = {Consistency and validity issues in consumer judgments},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2006},
Volume = {33},
Number = {3},
Pages = {291-303},
Month = {DEC},
Abstract = {Prior research has traced poor judgment quality to poor calibration. We
   suggest inconsistency to be another reason for poor judgment
   quality-utilizing different models on different occasions resulting in
   increased wandering in judgments. We demonstrate differing consistency
   in the utilization of models depending upon which variable is used as a
   cue and which is used as the criterion to be predicted. This results in
   differing correlations underlying judgments between the same two
   variables, an internally inconsistent pattern. We trace this to the
   utilization of lay causal models to make predictions but with the
   strength of the causal story moderating the consistency in use of the
   model.},
