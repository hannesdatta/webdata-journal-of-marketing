@article{ WOS:000252244300003,
Author = {Argo, Jennifer J. and Zhu, Rui (Juliet) and Dahl, Darren W.},
Title = {Fact or fiction: An investigation of empathy differences in response to
   emotional melodramatic entertainment},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2008},
Volume = {34},
Number = {5},
Pages = {614-623},
Month = {FEB},
Abstract = {Three studies investigate the influence of empathy and the level of
   fictionality of short stories on consumers' evaluations of emotional
   melodramatic entertainment. We find that high empathizers' evaluations
   are more favorable when the story is low in fictionality (i.e., real)
   versus high. In contrast, low empathizers' evaluations do not differ,
   regardless of the level of fictionality, except when these individuals
   (i.e., males) are provided with an excuse to become involved in the
   story; in this case a story that is high (i.e., make-believe) as opposed
   to low in fictionality is evaluated more favorably. Finally,
   transportation (i.e., absorption into a narrative) with the story is
   found to both moderate and mediate the effects.},
