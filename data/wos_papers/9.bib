@article{ WOS:000246541700007,
Author = {Herzenstein, Michal and Posavac, Steven S. and Brakus, J. Josko},
Title = {Adoption of new and really new products: The effects of self-regulation
   systems and risk salience},
Journal = {JOURNAL OF MARKETING RESEARCH},
