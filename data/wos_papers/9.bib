@article{ WOS:000241338600002,
Author = {Holzwarth, Martin and Janiszewski, Chris and Neumann, Marcus M.},
Title = {The influence of avatars on online consumer shopping behavior},
Journal = {JOURNAL OF MARKETING},
Year = {2006},
Volume = {70},
Number = {4},
Pages = {19-36},
Month = {OCT},
Abstract = {An impediment to Web-based retail sales is the impersonal nature of
   Web-based shopping. A solution to this problem is to use an avatar to
   deliver product information. An avatar is a graphic representation that
   can be animated by means of computer technology. Study 1 shows that
   using an avatar sales agent leads to more satisfaction with the
   retailer, a more positive attitude toward the product, and a greater
   purchase intention. Study 2 shows that an attractive avatar is a more
   effective sales agent at moderate levels of product involvement, but an
   expert avatar is a more effective sales agent at high levels of product
   involvement.},
