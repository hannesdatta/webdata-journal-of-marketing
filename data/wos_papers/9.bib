@article{ WOS:000249319800006,
Author = {Scott, Linda M. and Vargas, Patrick},
Title = {Writing with pictures: Toward a unifying theory of consumer response to
   images},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2007},
Volume = {34},
Number = {3},
Pages = {341-356},
Month = {OCT},
Abstract = {Studies of response to advertising images follow parallel streams: one
   treats visuals as sensory data; the other, operating under rhetorical
   theory, presumes that images are communicative artifacts. By revisiting
   a seminal article by Mitchell and Olson, we empirically demonstrate an
   alternative explanation for results under the sensory approach, while
   also establishing the basis for complex statements like tropes. We argue
