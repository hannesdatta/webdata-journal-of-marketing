@article{ WOS:000244158500010,
Author = {Luo, Xueming and Rindfleisch, Aric and Tse, David K.},
Title = {Working with rivals: The impact of competitor alliances on financial
   performance},
Journal = {JOURNAL OF MARKETING RESEARCH},
Year = {2007},
Volume = {44},
Number = {1},
Pages = {73-83},
Month = {FEB},
Abstract = {To survive and prosper in today's highly competitive environment, firms
   are increasingly engaging in cooperative alliances with their rivals.
   However, the impact of these competitor alliances on financial
   performance is largely unknown. This research examines this issue. Using
   both survey and archival data, the authors conduct two studies that
   reveal that the intensity of a firm's alliances with its competitors has
   a curvilinear (inverted U-shaped) influence on return on equity. In
   addition, the authors find that a firm's competitor orientation, as
   embodied in its strategies and objectives, can strengthen or weaken this
   curvilinear effect. Overall, these findings indicate that both
   competition and cooperation have dark sides that a firm must carefully
   manage when working with rivals.},
