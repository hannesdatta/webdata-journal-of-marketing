@article{ WOS:000250316600002,
Author = {Liu, Yuping},
Title = {The long-term impact of loyalty programs on consumer purchase behavior
   and loyalty},
Journal = {JOURNAL OF MARKETING},
Year = {2007},
Volume = {71},
Number = {4},
Pages = {19-35},
Month = {OCT},
Abstract = {Despite the prevalent use of loyalty programs, there is limited evidence
   on the long-term effects of such programs, and their effectiveness is
   not well established. The current research examines the long-term impact
   of a loyalty program on consumers' usage levels and their exclusive
   loyalty to the firm. Using longitudinal data from a convenience store
   franchise, the study shows that consumers who were heavy buyers at the
   beginning of a loyalty program were most likely to claim their qualified
   rewards, but the program did not prompt them to change their purchase
   behavior. In contrast, consumers whose initial patronage levels were low
   or moderate gradually purchased more and became more loyal to the firm.
   For light buyers, the loyalty program broadened their relationship with
   the firm into other business areas. The findings suggest a need to
   consider consumer idiosyncrasies when studying loyalty programs and
   illustrate consumers' cocreation of value in the marketing process.},
