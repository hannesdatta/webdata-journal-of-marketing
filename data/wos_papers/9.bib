@article{ WOS:000246591700008,
Author = {Murray, Kyle B. and Haubl, Gerald},
Title = {Explaining cognitive lock-in: The role of skill-based habits of use in
   consumer choice},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2007},
Volume = {34},
Number = {1},
Pages = {77-88},
Month = {JUN},
Abstract = {We introduce and test a theory of how the choices consumers make are
   influenced by skill-based habits of use-goal-activated automated
   behaviors that develop through the repeated consumption or use of a
   particular product. Such habits can explain how consumers become locked
   in to an incumbent product. The proposed theory characterizes how the
   amount of experience with the incumbent product, the occurrence of usage
   errors while learning to use that product, and the goal that is
   activated at the time a choice is made interrelate to influence consumer
   preference. The results of three experiments support the theory's
   predictions.},
