@article{ WOS:000240732700006,
Author = {Chernev, Alexander},
Title = {Differentiation and parity in assortment pricing},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2006},
Volume = {33},
Number = {2},
Pages = {199-210},
Month = {SEP},
Abstract = {Are consumers more likely to purchase an item from an assortment in
   which options are priced at parity or from an assortment in which
   options vary in price? This research examines the influence of
   parity-pricing and differentiation-pricing strategies on consumer choice
   and identifies conditions in which parity pricing facilitates choice, as
   well as conditions in which choice is facilitated by differential
   pricing. In a series of three experiments, the impact of assortment
   pricing on choice is shown to be a function of the uncertainty
   associated with consumers' preferences and the consistency of these
   preferences with options' prices.},
