@article{ WOS:000240732700011,
Author = {Bolton, Lisa E. and Alba, Joseph W.},
Title = {Price fairness: Good and service differences and the role of vendor
   costs},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2006},
Volume = {33},
Number = {2},
Pages = {258-265},
Month = {SEP},
Abstract = {Prior research suggests that consumers are forgiving of a price increase
   that is commensurate with increased vendor costs. We argue that the
   perceived fairness of the price increase will also depend on the
   alignability of the cost and price increases, such that alignable
   increases will be perceived as more acceptable than nonalignable
