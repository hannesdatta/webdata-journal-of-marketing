@article{ WOS:000248037600006,
Author = {Hamilton, Ryan and Hong, Jiewen and Chernev, Alexander},
Title = {Perceptual focus effects in choice},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2007},
Volume = {34},
Number = {2},
Pages = {187-199},
Month = {AUG},
Abstract = {This article examines consumer choice as a function of the perceptual
   similarity of the options in the decision set. In particular, we examine
   a scenario in which a set of options is extended by adding alternatives
   that change its perceptual characteristics, increasing the salience of
   one of the options in the core set. In this context, we document that,
   contrary to normative predictions, perceptual focus can increase the
   choice share of one of the core options, even when the added
   alternatives are dominated by both options in the core set. We further
   show that the observed effect is a function of consumers' mode of
   information processing and is more pronounced in the context of
   intuitive ( System 1) processing than analytic ( System 2) processing.},
