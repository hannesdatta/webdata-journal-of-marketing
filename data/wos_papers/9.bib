@article{ WOS:000252244300001,
Author = {Wong, Nancy and King, Tracey},
Title = {The cultural construction of risk understandings through illness
   narratives},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2008},
Volume = {34},
Number = {5},
Pages = {579-594},
Month = {FEB},
Abstract = {A study of breast cancer screening and treatment decisions suggests that
   risk understandings are influenced by the dominant illness narrative of
   restitution within Anglo-Western cultures. Restitution stories reflect
   the cultural values of personal responsibility and control in combating
   disease and returning to a life of normalcy. In the context of breast
   cancer, individuals seek restitution by following the dictums of
   biomedicine, which promotes early detection as prevention, aggressive
   treatment as cure, and reconstructive surgery as concealment. Our
   findings suggest that these risk understandings contribute to the
   consumption of health-care interventions that exceeds medical guidelines
   in this country.},
