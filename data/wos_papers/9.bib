@article{ WOS:000242126600008,
Author = {Janakiraman, Narayan and Meyer, Robert J. and Morales, Andrea C.},
Title = {Spillover effects: How consumers respond to unexpected changes in price
   and quality},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2006},
Volume = {33},
Number = {3},
Pages = {361-369},
Month = {DEC},
Abstract = {This article examines how unexpected changes in the marketing mix of one
   product in a retail setting can influence demand for other, unrelated,
   items. Results from two laboratory studies show that spillover effects
   can occur in response to both positive and negative changes in either
   the price or quality of a product, such that positive changes increase
   total spending on other items and negative changes reduce it. The
   results also demonstrate that an attributional process underlies these
   effects, indicating that consumers experience specific affective
   responses directed at the retailer that lead them either to reward or
   punish the retailer accordingly.},
