@article{ WOS:000246591700001,
Author = {Wertenbroch, Klaus and Soman, Dilip and Chattopadhyay, Amitava},
Title = {On the perceived value of money: The reference dependence of currency
   numerosity effects},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2007},
Volume = {34},
Number = {1},
Pages = {1-10},
Month = {JUN},
Abstract = {Money illusion research shows that the nominal (face) value of money
   affects consumer perceptions of its real value. Recent mixed findings on
   consumer valuations in different currencies suggest that the underlying
   anchoring and adjustment processes are complex. We develop a framework
   to identify boundary conditions that specify the direction of anchoring
   effects on valuations in different currencies. Consumers anchor on the
   numerosity of the nominal difference between prices and salient
   referents (e.g., budgets) when evaluating transactions. Support for our
   framework comes from a series of experiments that evoke different
   reference standards. We discuss implications and opportunities for
   future research.},
