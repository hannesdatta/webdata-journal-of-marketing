@article{ WOS:000240732700012,
Author = {Srivastava, Joydeep and Oza, Shweta},
Title = {Effect of response time on perceptions of bargaining outcomes},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2006},
Volume = {33},
Number = {2},
Pages = {266-272},
Month = {SEP},
Abstract = {This research reports three studies that examine how cues which emerge
   from the bargaining environment, such as the time taken by an opponent
   to respond to an offer, influence perceptions of bargaining outcomes.
   Study 1 finds that bargainers were more satisfied with outcomes when an
   offer was accepted after a delay than when accepted immediately. Study 2
   shows that inferences of the level of conflict within the opponent
   underlie the effect of response time on perceptions of bargaining
   outcomes. Study 3 shows that the presence of an objective ( or
   diagnostic) referent moderates the influence of response time.},
Publisher = {UNIV CHICAGO PRESS},
