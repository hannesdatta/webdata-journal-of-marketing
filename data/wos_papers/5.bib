@article{ WOS:000308452700008,
Author = {Lin, Ying-Ching and Chang, Chiu-chi Angela},
Title = {Double Standard: The Role of Environmental Consciousness in Green
   Product Usage},
Journal = {JOURNAL OF MARKETING},
Year = {2012},
Volume = {76},
Number = {5},
Pages = {125-134},
Month = {SEP},
Abstract = {The results from three studies suggest that consumers' perceptions of
   product effectiveness are critical in determining the amount of a
   product they choose to use in a given instance. In general, consumers
   consider green, or environmentally friendly, products to be less
   effective than regular products; therefore, consumers increase the
   amount of the green product they use to make up for the perceived
   inferiority. Notably, this pattern of green versus regular product usage
   is more pronounced among consumers who are environmentally conscious.
   When the perceived effectiveness of a green product is boosted by a
   credible endorsement, the discrepancy between green and regular product
   usage disappears.},
