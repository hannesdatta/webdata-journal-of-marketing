@article{ WOS:000322449000008,
Author = {Gu, Zheyin (Jane) and Liu, Yunchuan},
Title = {Consumer Fit Search, Retailer Shelf Layout, and Channel Interaction},
Journal = {MARKETING SCIENCE},
Year = {2013},
Volume = {32},
