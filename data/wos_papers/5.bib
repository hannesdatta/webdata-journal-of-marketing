@article{ WOS:000325305900002,
Author = {Rooderkerk, Robert P. and van Heerde, Harald J. and Bijmolt, Tammo H. A.},
Title = {Optimizing Retail Assortments},
Journal = {MARKETING SCIENCE},
Year = {2013},
