@article{ WOS:000315306400006,
Author = {Miklos-Thal, Jeanine and Zhang, Juanjuan},
Title = {(De)marketing to Manage Consumer Quality Inferences},
Journal = {JOURNAL OF MARKETING RESEARCH},
Year = {2013},
Volume = {50},
Number = {1},
Pages = {55-69},
Month = {FEB},
Abstract = {Savvy consumers attribute a product's market performance to its
   intrinsic quality as well as the seller's marketing push. The authors
   study how sellers should optimize their marketing decisions in response.
   They find that a seller can benefit from ``demarketing{''} its product,
   meaning visibly toning down its marketing efforts. Demarketing lowers
   expected sales ex ante but improves product quality image ex post, as
   consumers attribute good sales to superior quality and lackluster sales
   to insufficient marketing. The authors derive conditions under which
   demarketing can be a recommendable business strategy. A series of
   experiments confirm these predictions.},
Publisher = {AMER MARKETING ASSOC},
