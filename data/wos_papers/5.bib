@article{ WOS:000321701800001,
Author = {Coleman, Nicole Verrochi and Williams, Patti},
Title = {Feeling Like My Self: Emotion Profiles and Social Identity},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2013},
Volume = {40},
Number = {2},
Pages = {203-222},
Month = {AUG},
Abstract = {Individuals possess social identities that contain unique,
   identity-relevant attitudes, behaviors, and beliefs providing
   ``what-to-do{''} information when enacting that identity. We suggest
   that social identities are also associated with specific discrete
   emotion profiles providing ``what-to-feel{''} information during
   identity enactment. We show that consumers prefer emotional stimuli
   consistent with their salient social identity, make product choices and
   emotion regulating consumption decisions to enhance (reduce) their
   experience of identity-consistent (inconsistent) emotions, and that
   experiencing identity-consistent emotions aids in the performance of
   identity-relevant tasks.},
Publisher = {UNIV CHICAGO PRESS},
Address = {1427 E 60TH ST, CHICAGO, IL 60637-2954 USA},
