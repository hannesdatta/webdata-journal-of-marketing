@article{ WOS:000320682200012,
Author = {Kenrick, Douglas T. and Saad, Gad and Griskevicius, Vladas},
Title = {Evolutionary consumer psychology: Ask not what you can do for biology,
   but ...},
Journal = {JOURNAL OF CONSUMER PSYCHOLOGY},
Year = {2013},
Volume = {23},
Number = {3},
Pages = {404-409},
Month = {JUL},
Abstract = {The commentaries raise questions about modularity, and about the
   evidence required to establish evolutionary influences on behavior. We
   briefly discuss evidence leading evolutionary psychologists to assume
   that human choices reflect evolutionary influences, and to assume some
   degree of modularity in human information processing. An evolutionary
   perspective is based on a multidisciplinary nomological network of
   evidence, and results of particular experiments are only one part of
   that network. The precise nature of, and number of, information
   processing systems, is an empirical question. Consumer psychologists
   need not retrain as biologists to profit from using insights and
   findings from evolutionary biology to generate new hypotheses, and to
   contribute novel insights and findings to the emerging nomological
   network of modern evolutionary science. (C) 2013 Society for Consumer
   Psychology. Published by Elsevier Inc. All rights reserved.},
