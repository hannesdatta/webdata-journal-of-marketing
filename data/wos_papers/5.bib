@article{ WOS:000312417900006,
Author = {Yang, Sha and Hu, Mantian (Mandy) and Winer, Russell S. and Assael,
   Henry and Chen, Xiaohong},
Title = {An Empirical Study of Word-of-Mouth Generation and Consumption},
