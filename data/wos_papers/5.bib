@article{ WOS:000312417900007,
Author = {Sood, Ashish and James, Gareth M. and Tellis, Gerard J. and Zhu, Ji},
Title = {Predicting the Path of Technological Innovation: SAW vs. Moore, Bass,
   Gompertz, and Kryder},
Journal = {MARKETING SCIENCE},
Year = {2012},
