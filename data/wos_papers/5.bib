@article{ WOS:000319055700010,
Author = {Spassova, Gerri and Lee, Angela Y.},
Title = {Looking into the Future: A Match between Self-View and Temporal Distance},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2013},
Volume = {40},
Number = {1},
Pages = {159-171},
Month = {JUN},
Abstract = {Representing an event in abstract (vs. concrete) terms and as happening
   in the distant (vs. proximal) future has been shown to have important
   consequences for cognition and motivation. Less is known about factors
   that influence construal level and perceived temporal distance. The
   present research identifies one such factor and explores the
   implications for persuasion. Four studies show that an independent
   self-view is associated with abstract representations of future events
   and with perceiving these events as happening in the more distant
   future, whereas an interdependent self-view is associated with concrete
   representations of future events and with perceiving these events as
   happening in the more proximal future. Furthermore, a match (vs.
   mismatch) between the temporal frame of an advertisement and the
   self-view of the recipient leads to systematic changes in advertisement
   effectiveness and product appeal. These results add to the construal
   level theory and the self literatures and have practical implications
   for advertisers.},
