@article{ WOS:000327810600001,
Author = {Trusov, Michael and Rand, William and Joshi, Yogesh V.},
Title = {Improving Prelaunch Diffusion Forecasts: Using Synthetic Networks as
   Simulated Priors},
Journal = {JOURNAL OF MARKETING RESEARCH},
Year = {2013},
Volume = {50},
Number = {6},
Pages = {675-690},
Month = {DEC},
Abstract = {Although the role of social networks and consumer interactions in new
   product diffusion is widely acknowledged, such networks and interactions
   are often unobservable to researchers. What may be observable, instead,
   are aggregate diffusion patterns for past products adopted within a
   particular social network. The authors propose an approach for
   identifying systematic conditions that are stable across diffusions and
   thus are ``transferrable{''} to new product introductions within a given
   network. Using Facebook applications data, the authors show that
