@article{ WOS:000311764900002,
Author = {Ellickson, Paul B. and Misra, Sanjog and Nair, Harikesh S.},
Title = {Repositioning Dynamics and Pricing Strategy},
Journal = {JOURNAL OF MARKETING RESEARCH},
