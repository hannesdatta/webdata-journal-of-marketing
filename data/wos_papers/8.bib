@article{ WOS:000264952800006,
Author = {Hauser, John R. and Urban, Glen L. and Liberali, Guilherme and Braun,
   Michael},
Title = {Response to Comments on ``Website Morphing{''}},
Journal = {MARKETING SCIENCE},
Year = {2009},
Volume = {28},
Number = {2},
Pages = {227-228},
Month = {MAR-APR},
Abstract = {Website morphing draws on the Expected Gittins' solution to a partially
   observable Markov process, on the rapid consumer-segment updating with
   Bayesian methods, and on matching a website's look and feel to a
   visitor's cognitive style. In each area there are exciting research
   opportunities including optimality in the presence of switching costs
   (within a visit), Bayesian updating of cognitive styles across websites,
   extensions to other segmentation schemes such as cultural styles,
   morphing of other website characteristics such as advertising, and
   applications to other media such as smartphones.},
