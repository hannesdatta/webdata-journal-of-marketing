@article{ WOS:000265388900007,
Author = {Mishra, Arul},
Title = {Influence of Contagious versus Noncontagious Product Groupings on
   Consumer Preferences},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2009},
Volume = {36},
Number = {1},
Pages = {73-82},
Month = {JUN},
Abstract = {This article studies the influence of product groupings on consumer
   preferences. Specifically, it is proposed that when each product in two
   groups has an equal chance of a gain, consumers prefer to choose from a
   group that appears more contagious (e.g., products arranged close
   together, similarly, or symmetrically). However, when each product in
   two groups has an equal chance of a loss, consumers prefer to choose
   from a group that appears less contagious (e.g., products arranged
   apart, dissimilarly, or asymmetrically). Across three experiments, the
   effect is demonstrated, and contagion theory is used to explicate the
   underlying process.},
