@article{ WOS:000258767300014,
Author = {Markovitch, Dmitri G. and Golder, Peter N.},
Title = {Findings - Using stock prices to predict market events: Evidence on
   sales takeoff and long-term firm survival},
Journal = {MARKETING SCIENCE},
Year = {2008},
Volume = {27},
Number = {4},
Pages = {717-729},
Month = {JUL-AUG},
Abstract = {W e evaluate whether stock prices can predict the sales takeoff and the
   long-term survival of firms at takeoff. We find that abnormal returns
   are strongly positive in the year prior to takeoff, thus suggesting an
   important signal of the takeoff. Moreover, we find that negative
   abnormal returns in the year of takeoff and one year after takeoff
   increase the hazard of market exit by 9.5 times relative to firms
   without these negative abnormal returns. We discuss the implications of
   these findings for managers and researchers.},
