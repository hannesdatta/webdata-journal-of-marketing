@article{ WOS:000260877800010,
Author = {Haws, Kelly L. and Poynor, Cait},
Title = {Seize the Day! Encouraging Indulgence for the Hyperopic Consumer},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2008},
Volume = {35},
Number = {4},
Pages = {680-691},
Month = {DEC},
Abstract = {This article explores the phenomenon of ``hyperopia,{''} or an aversion
   to indulgence, as introduced by Kivetz and Keinan (2006) and Kivetz and
   Simonson (2002). We first develop a measure to capture hyperopia as an
   individual difference. Three empirical studies use this measure to
   demonstrate that hyperopia and high self-control are both conceptually
   and empirically distinct. Further, we show that altering the level at
   which an action or item is construed can make an indulgent goal or
   luxury product more appealing to the high hyperopia consumer by
   influencing its value in terms of an attractive long-term outcome.},
