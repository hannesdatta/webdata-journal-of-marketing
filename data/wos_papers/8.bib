@article{ WOS:000262871000006,
Author = {Zhu, Rui (Juliet) and Meyers-Levy, Joan},
Title = {The Influence of Self-View on Context Effects: How Display Fixtures Can
   Affect Product Evaluations},
Journal = {JOURNAL OF MARKETING RESEARCH},
Year = {2009},
Volume = {46},
Number = {1},
Pages = {37-45},
Month = {FEB},
Abstract = {Can the surface material of a display table prompt context effects on
   shoppers' product evaluations? If so, how might the direction of such
   effects be influenced by people's use of different modes of
   cognition-namely, holistic versus analytic cognition? The authors
   theorize and find that people's use of holistic cognition, as prompted
   by an interdependent self-view, produces an assimilation effect. Product
   evaluations are assimilated with associations with the table surface.
   However, people who rely on analytic processing, as prompted by an
   independent self-view, elicit a contrast effect in which evaluations are
   negatively related to such associations.},
