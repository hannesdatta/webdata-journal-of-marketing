@article{ WOS:000259289600003,
Author = {Scott, Maura L. and Nowlis, Stephen M. and Mandel, Naomi and Morales,
   Andrea C.},
Title = {The effects of reduced food size and package size on the consumption
   behavior of restrained and unrestrained eaters},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2008},
Volume = {35},
Number = {3},
Pages = {391-405},
Month = {OCT},
Note = {Transformatice Consumer Research Conference, Dartmouth Coll, Hanover,
   NH, 2007},
Abstract = {This research examines the moderating role of attempted dietary
   restraint on the amount of food consumed from small food in small
   packages versus large food in large packages. Four experiments
   demonstrate that restrained eaters consume more calories from small food
   in small packages, while unrestrained eaters consume more calories from
   large food in a large package. For restrained eaters, overconsumption of
   the small food in small packages results from a lapse in self-control
   caused by the stress of perceiving conflicting food information: the
   small food in small packages is perceived as both diet food and high in
   calories.},
