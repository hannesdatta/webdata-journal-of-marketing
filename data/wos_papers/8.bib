@article{ WOS:000263072000002,
Author = {Chen, Yuxin and Joshi, Yogesh V. and Raju, Jagmohan S. and Zhang, Z.
   John},
Title = {A Theory of Combative Advertising},
Journal = {MARKETING SCIENCE},
Year = {2009},
