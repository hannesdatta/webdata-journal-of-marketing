@article{ WOS:000256088600008,
Author = {Van den Bergh, Bram and Dewitte, Siegfried and Warlop, Luk},
Title = {Bikinis instigate generalized impatience in intertemporal choice},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2008},
Volume = {35},
Number = {1},
Pages = {85-97},
Month = {JUN},
Abstract = {Neuroscientific studies demonstrate that erotic stimuli activate the
   reward circuitry processing monetary and drug rewards. Theoretically, a
   general reward system may give rise to nonspecific effects: exposure to
   ``hot stimuli{''} from one domain may thus affect decisions in a
   different domain. We show that exposure to sexy cues leads to more
   impatience in intertemporal choice between monetary rewards.
   Highlighting the role of a general reward circuitry, we demonstrate that
   individuals with a sensitive reward system are more susceptible to the
   effect of sex cues, that the effect generalizes to nonmonetary rewards,
   and that satiation attenuates the effect.},
