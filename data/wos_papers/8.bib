@article{ WOS:000266291200011,
Author = {Hsee, Christopher K. and Yang, Yang and Li, Naihe and Shen, Luxi},
Title = {Wealth, Warmth, and Well-Being: Whether Happiness Is Relative or
   Absolute Depends on Whether It Is About Money, Acquisition, or
   Consumption},
Journal = {JOURNAL OF MARKETING RESEARCH},
Year = {2009},
Volume = {46},
Number = {3},
Pages = {396-409},
Month = {JUN},
Abstract = {A central question in consumer and happiness research is whether
   happiness depends on absolute or relative levels of wealth and
   consumption. To address this question, the authors evaluate a finer
   level than overall happiness and distinguish three specific types of
   happiness: with money, with the acquisition of an item, and with the
   consumption of an item. They find that happiness with money and with
   acquisition is relative and that happiness with consumption can be
   either absolute or relative, depending on whether the consumption is
   inherently evaluable or not. Including both lab and field data, this
   research yields implications for how to increase consumer happiness from
   one generation to the next.},
