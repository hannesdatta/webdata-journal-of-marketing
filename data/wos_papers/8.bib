@article{ WOS:000259289600004,
Author = {Sharpe, Kathryn M. and Staelin, Richard and Huber, Joel},
Title = {Using extremeness aversion to fight obesity: Policy implications of
   context dependent demand},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2008},
Volume = {35},
Number = {3},
Pages = {406-422},
Month = {OCT},
Abstract = {This article illustrates how the compromise effect alters consumers'
   selection of soft drinks. Using three within-subject studies, we show
   that extremeness aversion and price insensitivity cause consumers to
   increase their consumption when the smallest drink size is dropped or
   when a larger drink size is added to a set. As a result rational firms
   find it best to drop the smaller sizes and add a larger size, thus
   increasing overall consumption. After estimating each individual's
   demand as a function of price and drink size availability, policy
   experiments demonstrate how it is possible to reduce soft drink
   consumption without additional taxation.},
