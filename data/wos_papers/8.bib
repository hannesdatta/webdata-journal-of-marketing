@article{ WOS:000260625500009,
Author = {Du, Rex Y. and Kamakura, Wagner A.},
Title = {Where Did All That Money Go? Understanding How Consumers Allocate Their
   Consumption Budget},
Journal = {JOURNAL OF MARKETING},
Year = {2008},
Volume = {72},
