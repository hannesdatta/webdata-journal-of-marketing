@article{ WOS:000260310800008,
Author = {Mathwick, Charla and Wiertz, Caroline and De Ruyter, Ko},
Title = {Social Capital Production in a Virtual P3 Community},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2008},
Volume = {34},
Number = {6},
Pages = {832-849},
Month = {APR},
Abstract = {The purpose of this study is to examine the relational norms that
   determine social capital-an intangible resource embedded in and
   accumulated through a specific social structure. The social structure
   examined in this study is a virtual community created through text-based
   conversations oriented toward peer-to-peer problem solving (P3).
   Empirical results support the conceptualization of social capital as an
