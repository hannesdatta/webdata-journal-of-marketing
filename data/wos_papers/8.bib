@article{ WOS:000261527000005,
Author = {Nelson, Leif D. and Meyvis, Tom},
Title = {Interrupted Consumption: Disrupting Adaptation to Hedonic Experiences},
Journal = {JOURNAL OF MARKETING RESEARCH},
Year = {2008},
Volume = {45},
Number = {6},
Pages = {654-664},
Month = {DEC},
Abstract = {Six studies demonstrate that interrupting a consumption experience can
   make pleasant experiences more enjoyable and unpleasant experiences more
   irritating, even though consumers avoid breaks in pleasant experiences
   and choose breaks in unpleasant experiences. Across a variety of hedonic
