@article{ WOS:000269564200003,
Author = {Lee, Leonard and Amir, On and Ariely, Dan},
Title = {In Search of Homo Economicus: Cognitive Noise and the Role of Emotion in
   Preference Consistency},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2009},
Volume = {36},
Number = {2},
Pages = {173-187},
Month = {AUG},
Abstract = {Understanding the role of emotion in forming preferences is critical in
   helping firms choose effective marketing strategies and consumers make
   appropriate consumption decisions. In five experiments, participants
   made a set of binary product choices under conditions designed to induce
   different degrees of emotional decision processing. The results
   consistently indicate that greater reliance on emotional reactions
   during decision making is associated with greater preference consistency
   and less cognitive noise. Additionally, the results of a meta-analytical
   study based on data from all five experiments further show that products
   that elicit a stronger emotional response are more likely to yield
   consistent preferences.},
