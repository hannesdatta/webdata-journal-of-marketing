@article{ WOS:000257524900003,
Author = {Cayla, Julien and Eckhardt, Giana M.},
Title = {Asian brands and the shaping of a transnational imagined community},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2008},
Volume = {35},
Number = {2},
Pages = {216-230},
Month = {AUG},
Abstract = {We investigate how brand managers create regional Asian brands and show
   how some of them are attempting to forge new webs of interconnectedness
   through the construction of a transnational, imagined Asian world. Some
   branding managers are creating regional brands that emphasize the common
   experience of globalization, evoke a generic, hyper-urban, and
   multicultural experience, and are infused with diverse cultural
   referents. These types of regional Asian brands contribute to the
   creation of an imagined Asia as urban, modern, and multicultural.
   Understanding this process helps one to appreciate the role of branding
   managers in constructing markets and places.},
