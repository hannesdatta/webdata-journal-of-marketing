@article{ WOS:000593065700001,
Author = {Campbell, Margaret C. and Inman, J. Jeffrey and Kirmani, Amna and Price,
   Linda L.},
Title = {In Times of Trouble: A Framework for Understanding Consumers' Responses
   to Threats},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2020},
Volume = {47},
Number = {3},
Pages = {311-326},
Month = {OCT},
Abstract = {The COVID-19 pandemic and the accompanying economic downturn have
   dramatically impacted the lives of consumers around the world. From a
