@article{ WOS:000473648500003,
Author = {Yoon, Sangsuk and Fong, Nathan},
Title = {Uninformative Anchors Have Persistent Effects on Valuation Judgments},
Journal = {JOURNAL OF CONSUMER PSYCHOLOGY},
