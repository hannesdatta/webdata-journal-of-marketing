@article{ WOS:000593065700004,
Author = {Galoni, Chelsea and Carpenter, Gregory S. and Rao, Hayagreeva},
Title = {Disgusted and Afraid: Consumer Choices under the Threat of Contagious
   Disease},
Journal = {JOURNAL OF CONSUMER RESEARCH},
