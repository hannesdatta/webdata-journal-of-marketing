@article{ WOS:000468888000001,
Author = {de Jong, Martijn G. and Pieters, Rik},
Title = {Assessing Sensitive Consumer Behavior Using the Item Count Response
   Technique},
Journal = {JOURNAL OF MARKETING RESEARCH},
Year = {2019},
Volume = {56},
Number = {3},
Pages = {345-360},
Month = {JUN},
Abstract = {The authors propose a new truth-telling technique and statistical model
   called ``item count response technique{''} (ICRT) to assess the
   prevalence and drivers of sensitive consumer behavior. Monte Carlo
   simulations and a large-scale application to self-reported cigarette
   consumption among pregnant women (n = 1,315) demonstrate the
