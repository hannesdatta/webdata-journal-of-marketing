@article{ WOS:000513850000013,
Author = {Sahni, Navdeep S. and Nair, Harikesh S.},
Title = {Sponsorship Disclosure and Consumer Deception: Experimental Evidence
   from Native Advertising in Mobile Search},
Journal = {MARKETING SCIENCE},
Year = {2020},
Volume = {39},
