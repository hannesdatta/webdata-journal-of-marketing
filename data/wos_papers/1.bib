@article{ WOS:000581649700007,
Author = {Lee, Hyejin and Lalwani, Ashok K. and Wang, Jessie J.},
Title = {Price No Object!: The Impact of Power Distance Belief on Consumers'
   Price Sensitivity},
Journal = {JOURNAL OF MARKETING},
