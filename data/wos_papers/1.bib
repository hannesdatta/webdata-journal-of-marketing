@article{ WOS:000482137800005,
Author = {Morewedge, Carey K. and Zhu, Meng and Buechel, Eva C.},
Title = {Hedonic Contrast Effects Are Larger When Comparisons Are Social},
Journal = {JOURNAL OF CONSUMER RESEARCH},
