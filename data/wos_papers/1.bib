@article{ WOS:000533818200005,
Author = {Cian, Luca and Longoni, Chiara and Krishna, Aradhna},
Title = {Advertising a Desired Change: When Process Simulation Fosters (vs.
   Hinders) Credibility and Persuasion},
Journal = {JOURNAL OF MARKETING RESEARCH},
