@article{ WOS:000507377400005,
Author = {Deliema, Marguerite and Shadel, Doug and Pak, Karla},
Title = {Profiling Victims of Investment Fraud: Mindsets and Risky Behaviors},
Journal = {JOURNAL OF CONSUMER RESEARCH},
