@article{ WOS:000473252500002,
Author = {Ishihara, Masakazu and Ching, Andrew T.},
Title = {Dynamic Demand for New and Used Durable Goods Without Physical
   Depreciation: The Case of Japanese Video Games},
Journal = {MARKETING SCIENCE},
