@article{ WOS:000553023100001,
Author = {Farmer, Adam and Kidwell, Blair and Hardesty, David M.},
Title = {The Politics of Choice: Political Ideology and Intolerance of Ambiguity},
Journal = {JOURNAL OF CONSUMER PSYCHOLOGY},
Year = {2021},
Volume = {31},
Number = {1},
Pages = {6-21},
Month = {JAN},
Abstract = {The authors examine how political ideology impacts consumer preferences
   for hedonic and utilitarian choices and the underlying reasons for these
   differences. Five studies indicate that conservatives are less tolerant
   of ambiguity than liberals, leading to a preference for utilitarian
   options, whereas liberals are more tolerant of ambiguity leading to a
   preference for hedonic options. However, these preferences were reversed
   when utilitarian options were framed as ambiguous and hedonic options
   were framed as explicit and clear.},
