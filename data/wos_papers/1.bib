@article{ WOS:000486226600001,
Author = {Ellickson, Paul B. and Lovett, Mitchell J. and Ranjan, Bhoomija},
Title = {Product Launches with New Attributes: A Hybrid Conjoint-Consumer Panel
   Technique for Estimating Demand},
Journal = {JOURNAL OF MARKETING RESEARCH},
