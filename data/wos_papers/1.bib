@article{ WOS:000542330500006,
Author = {Sevilla, Julio and Meyer, Robert J.},
Title = {Leaving Something for the Imagination: The Effect of Visual Concealment
   on Preferences},
Journal = {JOURNAL OF MARKETING},
Year = {2020},
Volume = {84},
Number = {4},
Pages = {109-126},
Month = {JUL},
Abstract = {When advertising products to consumers, firms sometimes conceal key
   aspects in an effort to arouse consumer curiosity. This research
   investigates when and how visual concealment tactics may benefit or hurt
   aesthetic product evaluations. The authors propose that when consumers
   are only able to view a portion of an aesthetic product, assessments of
   its appeal will be influenced by two interrelated mechanisms: curiosity
   to see the item completed and inferences about the item's fully
   disclosed appearance. The authors show that heightened curiosity
   triggers feelings of positive affect that are transferred to the product
   itself, a process that may inflate preferences and choice likelihoods
   for products beyond what would occur if the full image were known. This
   transference effect, however, has an important boundary: it works only
   when initial consumer inferences about the appeal of the product are
   positive or emotionally congruent with the positive affect triggered by
   curiosity. The key implication is that, ironically, the products likely
   to benefit most from concealment tactics are those that have the least
   to hide. The authors provide evidence for these effects and the
   underlying mechanism using six experiments that manipulate concealment
   in a variety of task settings.},
Publisher = {SAGE PUBLICATIONS INC},
Address = {2455 TELLER RD, THOUSAND OAKS, CA 91320 USA},
Type = {Article},
Language = {English},
Affiliation = {Sevilla, J (Corresponding Author), Univ Georgia, Terry Coll Business, Mkt, Athens, GA 30602 USA.
   Sevilla, Julio, Univ Georgia, Terry Coll Business, Mkt, Athens, GA 30602 USA.
   Meyer, Robert J., Univ Penn, Wharton Sch, Mkt, Philadelphia, PA 19104 USA.},
DOI = {10.1177/0022242919899393},
ISSN = {0022-2429},
EISSN = {1547-7185},
Keywords = {curiosity; affect; aesthetics; online retailing; advertising},
