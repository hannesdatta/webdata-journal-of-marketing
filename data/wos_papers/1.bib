@article{ WOS:000486226600002,
Author = {Fisher, Marshall L. and Gallino, Santiago and Xu, Joseph Jiaqi},
Title = {The Value of Rapid Delivery in Omnichannel Retailing},
Journal = {JOURNAL OF MARKETING RESEARCH},
