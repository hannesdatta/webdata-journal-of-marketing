@article{ WOS:000556769700006,
Author = {Stourm, Ludovic and Iyengar, Raghuram and Bradlow, Eric T.},
Title = {A Flexible Demand Model for Complements Using Household Production
   Theory},
Journal = {MARKETING SCIENCE},
Year = {2020},
