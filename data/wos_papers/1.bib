@article{ WOS:000474704400006,
Author = {DeRosia, Eric D. and Elder, Ryan S.},
Title = {Harmful Effects of Mental Imagery and Customer Orientation During New
   Product Screening},
Journal = {JOURNAL OF MARKETING RESEARCH},
Year = {2019},
Volume = {56},
Number = {4},
Pages = {637-651},
Month = {AUG},
Abstract = {One of the traditional tenets of marketing is that managers considering
   whether to develop and launch a new product should adopt a customer
   orientation and consider whether the product would satisfy the needs of
   customers. This research discovers that adopting a customer orientation
   causes managers to experience undesirable cognitive effects. The authors
   find that when considering customers' needs during the screening phase
   of the new product development process, managers often voluntarily
   engage in mental imagery (i.e., cognitive simulation) that biases their
   evaluation of a new product idea toward unrealistic optimism-even for a
   flawed product that would not satisfy customer needs. Furthermore, the
   authors find that managers who exert greater vigilance to achieve more
