@article{ WOS:000473648500004,
Author = {Roose, Gudrun and Vermeir, Iris and Geuens, Maggie and Van Kerckhove,
   Anneleen},
Title = {A Match Made in Heaven or Down Under? The Effectiveness of Matching
   Visual and Verbal Horizons in Advertising},
Journal = {JOURNAL OF CONSUMER PSYCHOLOGY},
Year = {2019},
Volume = {29},
Number = {3},
Pages = {411-427},
Month = {JUL},
Abstract = {According to construal level theory, consumers' processing styles become
   more abstract as psychological distance increases, but they grow more
   concrete when psychological distance decreases. This study provides
   evidence that panoramic pictures in which the horizon appears in the
   lower versus upper part of the frame trigger a different level of
