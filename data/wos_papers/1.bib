@article{ WOS:000544020000007,
Author = {Botner, Keith A. and Mishra, Arul and Mishra, Himanshu},
Title = {The Influence of the Phonetic Elements of a Name on Risk Assessment},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2020},
Volume = {47},
Number = {1},
Pages = {128-145},
Month = {JUN},
Abstract = {The authors propose that the phonetic elements of a name affect risk
   perception. Specifically, they find that people prefer a name that
   evokes volatility when faced with a risky prospect, but prefer a name
   that evokes calmness when faced with a safe prospect. The authors posit
   that a volatile (vs. calm) prospect name results in more perceived
   fluctuations, and thus greater movement from, the given risk level.
   Therefore, a volatile prospect name results in a wider range of
