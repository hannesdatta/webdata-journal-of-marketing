@article{ WOS:000559818700001,
Author = {Bhagwat, Yashoda and Warren, Nooshin L. and Beck, Joshua T. and Watson,
   George F.},
Title = {Corporate Sociopolitical Activism and Firm Value},
Journal = {JOURNAL OF MARKETING},
Year = {2020},
Volume = {84},
