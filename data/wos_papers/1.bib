@article{ WOS:000468888000004,
Author = {Sahni, Navdeep S. and Narayanan, Sridhar and Kalyanam, Kirthi},
Title = {An Experimental Investigation of the Effects of Retargeted Advertising:
   The Role of Frequency and Timing},
Journal = {JOURNAL OF MARKETING RESEARCH},
