@article{ WOS:000507375800006,
Author = {Okada, Erica Mina},
Title = {Differential Construal of Exercise versus Diet and Implications for
   Weight Control},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2019},
Volume = {46},
Number = {3},
Pages = {528-544},
Month = {OCT},
Abstract = {Successful weight control is a challenge for many consumers, but in
   principle, it is a simple arithmetic combination of increasing caloric
   expenditure and/or decreasing caloric intake. We call the former
   ``exercise{''} and the latter ``diet.{''} A field survey, three
   behavioral lab experiments, and a national weight loss field study show
   that (1) people tend to construe exercise at higher levels and diet at
   lower levels, (2) activating people's focus on the high-level purposes
   versus low-level processes of their behavior may improve the odds of
