@article{ WOS:000473648500015,
Author = {Price, Linda L. and Coulter, Robin A.},
Title = {Crossing Bridges: Assembling Culture into Brands and Brands into
   Consumers' Global Local Cultural Lives},
Journal = {JOURNAL OF CONSUMER PSYCHOLOGY},
