@article{ WOS:000473648500013,
Author = {Fournier, Susan and Alvarez, Claudio},
Title = {How Brands Acquire Cultural Meaning},
Journal = {JOURNAL OF CONSUMER PSYCHOLOGY},
Year = {2019},
Volume = {29},
Number = {3},
Pages = {519-534},
Month = {JUL},
Abstract = {This article endeavors to advance research on the cultural resonance of
   brands by building bridges between branding scholarship in the consumer
   psychology tradition and interpretive research regarding brands and
   their meaning makers. We adopt a cognitivist conceptualization of
   cultural meaning and focus on the application of interpretive insights
   to well-established constructs in the consumer psychology of brands:
   brand associations, product category associations, social identity, and
   self-identity. This integrative exercise highlights the value of
   cultural models in explaining the processes whereby brands acquire
   meaning, and suggests several themes that are under-valued when
   considering this process problem through a psychological lens: the
   motivational underpinnings of myths and other cultural meaning models,
   the relative value of shared cultural and brand meanings versus
   idiosyncratic meanings, the power and primacy of category-level meaning
   making over brand-level meaning making, the complex processes whereby
   brands gain and lose legitimacy, and the influence of lay theories about
   brands and branding on how consumers co-create meaning for brands.},
Publisher = {JOHN WILEY \& SONS LTD},
