@article{ WOS:000307492000004,
Author = {Galbreth, Michael R. and Ghosh, Bikram and Shor, Mikhael},
Title = {Social Sharing of Information Goods: Implications for Pricing and
   Profits},
Journal = {MARKETING SCIENCE},
Year = {2012},
Volume = {31},
