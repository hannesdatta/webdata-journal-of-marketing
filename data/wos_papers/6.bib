@article{ WOS:000292065800013,
Author = {MacInnis, Deborah J.},
Title = {A Framework for Conceptual Contributions in Marketing},
Journal = {JOURNAL OF MARKETING},
Year = {2011},
Volume = {75},
Number = {4},
Pages = {136-154},
Month = {JUL},
Abstract = {Conceptual advances are critical to the vitality of the marketing
   discipline, yet recent writings suggest that conceptual advancement in
   the field is slowing. The author addresses this issue by developing a
   framework for thinking about conceptualization in marketing. A
   definition of conceptualization is followed by a typology of types of
   conceptual contributions. The types of conceptual contributions, their
   similarities and differences, and their importance to the field are
   described. Thinking skills linked to various types of conceptual
   contributions are also described, as are the use of tools that can
   facilitate these skills. The article concludes with a set of
   recommendations for advancing conceptualization in our field in the
   years to come.},
