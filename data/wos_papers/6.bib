@article{ WOS:000291010200001,
Author = {Goldfarb, Avi and Tucker, Catherine},
Title = {Online Display Advertising: Targeting and Obtrusiveness},
Journal = {MARKETING SCIENCE},
Year = {2011},
Volume = {30},
Number = {3},
Pages = {389-404},
Month = {MAY-JUN},
Abstract = {W e use data from a large-scale field experiment to explore what
   influences the effectiveness of online advertising. We find that
   matching an ad to website content and increasing an ad's obtrusiveness
   independently increase purchase intent. However, in combination, these
   two strategies are ineffective. Ads that match both website content and
   are obtrusive do worse at increasing purchase intent than ads that do
   only one or the other. This failure appears to be related to privacy
   concerns: the negative effect of combining targeting with obtrusiveness
   is strongest for people who refuse to give their income and for
   categories where privacy matters most. Our results suggest a possible
   explanation for the growing bifurcation in Internet advertising between
   highly targeted plain text ads and more visually striking but less
   targeted ads.},
