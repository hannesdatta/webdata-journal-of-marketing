@article{ WOS:000289596700001,
Author = {Priester, Joseph R. and Petty, Richard E.},
Title = {The pot-holed path to happiness, possibly paved with money: A research
   dialogue},
Journal = {JOURNAL OF CONSUMER PSYCHOLOGY},
Year = {2011},
Volume = {21},
Number = {2},
Pages = {113-114},
Month = {APR},
Abstract = {The path to happiness is not always clear. Research suggests that even
   money (when one has enough to take care of basic needs) does not enhance
   happiness, This Research Dialogue explores how individuals can take
