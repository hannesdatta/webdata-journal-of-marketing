@article{ WOS:000291076900002,
Author = {Katona, Zsolt and Zubcsek, Peter Pal and Sarvary, Miklos},
Title = {Network Effects and Personal Influences: The Diffusion of an Online
   Social Network},
Journal = {JOURNAL OF MARKETING RESEARCH},
