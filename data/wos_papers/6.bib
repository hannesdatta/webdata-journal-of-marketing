@article{ WOS:000293824200013,
Author = {Jiang, Yuanchun and Shang, Jennifer and Kemerer, Chris F. and Liu,
   Yezheng},
Title = {Optimizing E-tailer Profits and Customer Savings: Pricing Multistage
   Customized Online Bundles},
Journal = {MARKETING SCIENCE},
