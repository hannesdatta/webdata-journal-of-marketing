@article{ WOS:000286867600015,
Author = {Mishra, Arul and Mishra, Himanshu},
Title = {The Influence of Price Discount Versus Bonus Pack on the Preference for
   Virtue and Vice Foods},
Journal = {JOURNAL OF MARKETING RESEARCH},
Year = {2011},
Volume = {48},
Number = {1},
Pages = {196-206},
Month = {FEB},
Abstract = {The authors find that consumers prefer a bonus pack to a price discount
   for virtue foods but they prefer a price discount to a bonus pack for
   vice foods. Prior research has shown that, all else being equal,
   consumers prefer bonus packs to price discounts. The authors propose
   that this preference does not hold for vice food, because consumers
   cannot generate good justifications for buying such food when a bonus
   pack is offered because this would mean consuming more of the vice.
