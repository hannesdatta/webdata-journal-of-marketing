@article{ WOS:000299112100010,
Author = {Herbst, Kenneth C. and Finkel, Eli J. and Allan, David and Fitzsimons,
   Grainne M.},
Title = {On the Dangers of Pulling a Fast One: Advertisement Disclaimer Speed,
   Brand Trust, and Purchase Intention},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2012},
Volume = {38},
Number = {5},
Pages = {909-919},
Month = {FEB},
Abstract = {Two experiments demonstrated that fast (vs. normal-paced)
   end-of-advertisement disclaimers undermine consumers' purchase intention
   toward untrusted brands (both trust-unknown and not-trusted brands), but
   that disclaimer speed has no effect on consumers' purchase intention
   toward trusted brands. The differential effects of disclaimer speed for
   untrusted versus trusted brands were not due to differences in
   consumers' familiarity with the brands (experiment 1). Consistent with
   the hypothesis that fast disclaimers adversely affect purchase intention
   via heuristic rather than elaborative processes, the disclaimer speed x
   brand trust interaction effect remained robust even when the disclaimer
   presented positive information about the advertised product (experiment
   2).},
