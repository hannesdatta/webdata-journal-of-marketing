@article{ WOS:000304383000014,
Author = {Boland, Wendy Attaya and Connell, Paul M. and Erickson, Lance-Michael},
Title = {Children's response to sales promotions and their impact on purchase
   behavior},
Journal = {JOURNAL OF CONSUMER PSYCHOLOGY},
Year = {2012},
Volume = {22},
Number = {2},
Pages = {272-279},
Month = {APR},
Abstract = {Despite the growth in children's purchasing power, surprisingly little
   is known about how children respond to sales promotions. We conduct two
   experiments to address this issue. Study 1 shows that elementary-aged
   (second and fifth grade) children's purchase decisions are influenced by
   the presence of sales. Study 2 demonstrates that both age groups favor
   conceptually easier promotions, even when the sale is inferior.
