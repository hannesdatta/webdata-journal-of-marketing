@article{ WOS:000292340400005,
Author = {Fisher, Robert J. and Gregoire, Yany and Murray, Kyle B.},
Title = {The limited effects of power on satisfaction with joint consumption
   decisions},
Journal = {JOURNAL OF CONSUMER PSYCHOLOGY},
Year = {2011},
Volume = {21},
Number = {3},
Pages = {277-289},
Month = {JUL},
Abstract = {We conduct three experiments in which participants in dyads choose
   between two restaurants, each of which is preferred by only one
   participant, and one participant has the power to decide which
   restaurant both will patronize. We find that the power to make a joint
   decision increases satisfaction with the choice only when those involved
   have a competitive decision orientation, a weak or anonymous
   relationship, and the outcome they choose is subsequently available.
   Participants who have a cooperative orientation are satisfied whether or
   not they have power and whether or not the resulting choice is
   consistent with their initial preferences. (C) 2011 Society for Consumer
   Psychology. Published by Elsevier Inc. All rights reserved.},
