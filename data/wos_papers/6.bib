@article{ WOS:000301356900010,
Author = {Argo, Jennifer J. and Shiv, Baba},
Title = {Are White Lies as Innocuous as We Think?},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2012},
Volume = {38},
Number = {6},
Pages = {1093-1102},
Month = {APR},
Abstract = {This research examines the implications of telling an ``innocent{''}
   white lie after a negative interpersonal encounter. We propose that if a
   white lie falls outside an acceptable range of dishonesty, cognitive
   dissonance will arise and produce negative affect. Deceivers will then
   be motivated to reduce the dissonance and will do so by engaging in
   behaviors that favor the wrongdoer with potentially negative
   consequences for the self. We test our conceptualization across three
   studies. In study 1, we explore the impact of one factor that determines
   whether a white lie falls outside the acceptable range of dishonesty-the
   salience of the norm of honesty. In studies 2 and 3, we examine the role
   of two factors, affect certainty and source certainty, that are
   predicted to moderate the impact of the negative affect on deceiver's
   downstream judgments and behaviors toward the target of the white lie.},
