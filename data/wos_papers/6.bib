@article{ WOS:000299112100002,
Author = {Mishra, Arul and Mishra, Himanshu and Masters, Tamara M.},
Title = {The Influence of Bite Size on Quantity of Food Consumed: A Field Study},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2012},
Volume = {38},
Number = {5},
Pages = {791-795},
Month = {FEB},
Abstract = {While research has extensively investigated how portion sizes can
   influence the quantity of food consumed, relatively little work has been
   done to explore how bite size influences overall consumption. This
   research seeks to address this concern. In a field study, we collected
   data in a restaurant and manipulated bite size by providing diners with
   small or large forks. We found that diners consumed more from smaller
   rather than larger forks. Utilizing motivation literature, which ties
   into the unique factors present in a restaurant consumption setting (e.
   g., diners have a well-defined goal of hunger satiation because they
   invest effort by visiting a specific restaurant, choose from a menu, and
   pay money for the meal), we present our rationale for the pattern of
   results. Moreover, in a controlled lab study we demonstrate that when
   these factors are absent, the pattern of results is reversed.},
