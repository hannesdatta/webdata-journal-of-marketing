@article{ WOS:000292340400012,
Author = {Morrin, Maureen and Krishna, Aradhna and Lwin, May O.},
Title = {Is scent-enhanced memory immune to retroactive interference?},
Journal = {JOURNAL OF CONSUMER PSYCHOLOGY},
