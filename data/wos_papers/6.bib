@article{ WOS:000293049100004,
Author = {Li, Shibo and Sun, Baohong and Montgomery, Alan L.},
Title = {Cross-Selling the Right Product to the Right Customer at the Right Time},
Journal = {JOURNAL OF MARKETING RESEARCH},
