@article{ WOS:000291076900001,
Author = {Puntoni, Stefano and Sweldens, Steven and Tavassoli, Nader T.},
Title = {Gender Identity Salience and Perceived Vulnerability to Breast Cancer},
Journal = {JOURNAL OF MARKETING RESEARCH},
Year = {2011},
Volume = {48},
