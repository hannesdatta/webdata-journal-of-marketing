@article{ WOS:000357703600006,
Author = {Klein, Jill G. and Lowrey, Tina M. and Otnes, Cele C.},
Title = {Identity-based motivations and anticipated reckoning: Contributions to
   gift-giving theory from an identity-stripping context},
Journal = {JOURNAL OF CONSUMER PSYCHOLOGY},
Year = {2015},
Volume = {25},
Number = {3},
Pages = {431-448},
Month = {JUL},
Abstract = {We utilize the Identity-Based Motivation (IBM) model to examine gift
   giving within the identity-stripping context of Nazi concentration
   camps, as reported in the memoirs of Holocaust survivors. By exploring
   gift giving in this crisis-laden context, we demonstrate the fundamental
   role gifts can play in reestablishing personal and social identities. In
   doing so, we provide insights into the motivations for giving that go
   beyond the existing paradigms that emphasize social exchange, economic
   exchange, or agapic giving. Further, we introduce the construct of
   anticipated reckoning, in which people self-regulate their behavior
   through an imagined future self whom they perceive to judge their
   current actions. (C) 2015 Society for Consumer Psychology. Published by
   Elsevier Inc. All rights reserved.},
