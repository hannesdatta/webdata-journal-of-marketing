@article{ WOS:000362390100013,
Author = {McClelland, Gary H. and Lynch, Jr., John G. and Irwin, Julie R. and
   Spiller, Stephen A. and Fitzsimons, Gavan J.},
Title = {Median splits, Type II errors, and false-positive consumer psychology:
   Don't fight the power},
Journal = {JOURNAL OF CONSUMER PSYCHOLOGY},
Year = {2015},
Volume = {25},
Number = {4},
Pages = {679-689},
Month = {OCT},
Abstract = {Considerable prior statistical work has criticized replacing a
   continuously measured variable in a general linear model with a
   dichotomy based on a median split of that variable. lacobucci, Posovac,
   Kardes, Schneider, and Popovich (2015-in this issue) defend the practice
   of ``median splits{''} using both conceptual arguments and simulations.
   We dispute their conceptual arguments, and we have identified technical
   errors in their simulations that dramatically change the conclusions
   that follow from those simulations. We show that there are no real
   benefits to median splits, and there are real costs in increases in Type
   II errors through loss of power and increases in Type 1 errors through
   false positive consumer psychology. We conclude that median splits
   remain a bad idea. (C) 2015 Society for Consumer Psychology. Published
   by Elsevier Inc. All rights reserved.},
