@article{ WOS:000352673600012,
Author = {Besharat, Ali and Varki, Sajeev and Craig, Adam W.},
Title = {Keeping consumers in the red: Hedonic debt prioritization within
   multiple debt accounts},
Journal = {JOURNAL OF CONSUMER PSYCHOLOGY},
Year = {2015},
Volume = {25},
Number = {2},
Pages = {311-316},
Month = {APR},
Abstract = {In our paper we contribute to the burgeoning literature in the
   psychology of debt repayment. Across three experiments, we explore the
   effects of the type (hedonic or utilitarian) and the timing of debt on
   consumers' debt repayment when managing multiple debt accounts. While
   prior literature has demonstrated that debtors who own multiple credit
   cards behave irrationally by paying down smaller balances rather than
   balances with higher interest rates, we found that debts incurred for
   hedonic purchases and in the distant past (versus proximal past) amplify
   this effect. The anticipated impact of debt repayment on consumption
   enjoyment is found to mediate this effect. (C) 2014 Society for Consumer
   Psychology. Published by Elsevier Inc. All rights reserved.},
