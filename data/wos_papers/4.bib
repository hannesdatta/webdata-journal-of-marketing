@article{ WOS:000360847100002,
Author = {O'Guinn, Thomas Clayton and Tanner, Robin J. and Maeng, Ahreum},
Title = {Turning to Space: Social Density, Social Class, and the Value of Things
   in Stores},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2015},
Volume = {42},
Number = {2},
Pages = {196-213},
Month = {AUG},
Abstract = {This article is about social space and material objects for sale within
   that space. We draw primarily on Goffman's (1971) concepts of use space
   and possession territories to predict that as the social density of a
   given space increases, inferences of the subjective social class and
   income of people in that space fall. Eight studies confirm that this is
   indeed the case, with the result holding even for stick figures, thus
   controlling for typical visual indicators of social class such as
   clothing or jewelry. Furthermore, these social class inferences mediate
   a relationship between social density and product valuation, with
   individuals assessing both higher prices and a greater willingness to
   pay for products presented in less crowded contexts. This effect of
   inferred class on product valuation is explained by status-motivated
   individuals' desire to associate with higher-status people. To the best
   of our knowledge, this research is the first to reveal the link between
   social density, status inferences, and object valuations. As such, it
   makes a novel contribution to what has come to be known in sociology as
   the topological turn: a renewed focus on social space.},
