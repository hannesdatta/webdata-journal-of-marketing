@article{ WOS:000339171400012,
Author = {Fisher, Robert J. and Ma, Yu},
Title = {The Price of Being Beautiful: Negative Effects of Attractiveness on
   Empathy for Children in Need},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2014},
Volume = {41},
Number = {2},
Pages = {436-450},
Month = {AUG},
Abstract = {The research examines how the attractiveness of children in need affects
   the empathy they evoke and the subsequent help they receive from
   unrelated adults. The authors find that attractive children are
   attributed desirable characteristics related to social competence, which
   is consistent with the ``beautiful is good{''} stereotype. Ironically,
   the authors find that these attributions reduce the empathy evoked by
   attractive children and the help they receive from unrelated adults as
   long as their need is not severe. These effects are demonstrated in four
   experiments. The research identifies a significant cost of being
   beautiful and an important exception to the beautiful is good
