@article{ WOS:000351661900002,
Author = {Zhang, Yao and Bradlow, Eric T. and Small, Dylan S.},
Title = {Predicting Customer Value Using Clumpiness: From RFM to RFMC},
Journal = {MARKETING SCIENCE},
Year = {2015},
Volume = {34},
