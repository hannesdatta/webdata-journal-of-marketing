@article{ WOS:000345227600004,
Author = {Mittelman, Mauricio and Andrade, Eduardo B. and Chattopadhyay, Amitava
   and Brendl, C. Miguel},
Title = {The Offer Framing Effect: Choosing Single versus Bundled Offerings
   Affects Variety Seeking},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2014},
Volume = {41},
Number = {4},
Pages = {953-964},
Month = {DEC},
Abstract = {Choices of multiple items can be framed as a selection of single
   offerings (e. g., a choice of two individual candy bars) or of bundled
   offerings (e. g., a choice of a bundle of two candy bars). Four
   experiments provide strong evidence that consumers seek more variety
   when choosing from single than from bundled offerings. The offer framing
   effect shows that the mechanics of choosing-the ways consumers go about
   making choices of multiple items-affect variety seeking in a systematic
   manner. The data also suggest that the effect is largely due to the
   single offering frame. Theoretical and managerial implications are
   discussed.},
