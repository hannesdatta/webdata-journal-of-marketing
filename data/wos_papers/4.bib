@article{ WOS:000340580300004,
Author = {Walsh, Darlene},
Title = {Attenuating depletion using goal priming},
Journal = {JOURNAL OF CONSUMER PSYCHOLOGY},
Year = {2014},
Volume = {24},
Number = {4},
Pages = {497-505},
Month = {OCT},
Abstract = {This research examines how goal priming can attenuate the depletion
   effect. Using different self-control goals (i.e., savings and healthy
   eating) and different measures of self-control ability (i.e.,
   willingness to buy and actual consumption), this study reveals that when
   people were primed with cues related to a self-control goal and then
   depleted, the effect of depletion on a subsequent self-control task
   (related to the primed goal) became attenuated. Also, depleted people,
   relative to nondepleted people, reported a lower level of commitment to
   a self-control goal; however, when cues related to a self-control goal
   were primed, their level of goal commitment increased, weakening the
   depletion effect. This research clarifies questions related to the
   process underlying depletion, while highlighting the importance of goal
   commitment (a measure of motivation) in understanding depletion. (C)
   2014 Society for Consumer Psychology. Published by Elsevier Inc. All
   rights reserved.},
