@article{ WOS:000362390100001,
Author = {Han, Dahee and Duhachek, Adam and Rucker, Derek D.},
Title = {Distinct threats, common remedies: How consumers cope with psychological
   threat},
Journal = {JOURNAL OF CONSUMER PSYCHOLOGY},
Year = {2015},
Volume = {25},
Number = {4},
Pages = {531-545},
Month = {OCT},
Abstract = {Research has consistently demonstrated that psychological threats to the
   self have a wide variety of consequences for consumer behavior. The
   present research introduces a novel perspective to this topic by
   proposing that psychologically distinct domains of threat have a common
   underpinning in the coping strategies they evoke. Specifically, this
   paper presents the argument that distinct domains of threat can be
   linked to either approach motivations that foster more problem-focused
   coping or avoidance motivations that foster more emotion-focused coping.
   Multiple experiments offer systematic support for these propositions.
   Implications for both the psychological self-threat literature and the
   coping literature are discussed. (C) 2015 Society for Consumer
   Psychology. Published by Elsevier Inc. All rights reserved.},
