@article{ WOS:000340580300011,
Author = {Morwitz, Vicki G.},
Title = {Insights from the animal kingdom},
Journal = {JOURNAL OF CONSUMER PSYCHOLOGY},
Year = {2014},
Volume = {24},
Number = {4},
Pages = {572-585},
Month = {OCT},
Abstract = {Just as we have learned a great deal in consumer psychology by focusing
   on understanding how different sub-groups of humans think, this paper
   suggests that we can also learn from examining how different types of
   animals think. To that end, this manuscript offers a review of
   literature on topics in animal cognition that have also been
   investigated by consumer researchers. It first reviews research that has
   identified ways in which animals and humans are similar and then reviews
   research that has identified ways in which animals differ from humans,
   with a focus on ways in which some animals have been shown to outperform
   humans. The manuscript concludes with a discussion of opportunities for
   future research. (C) 2014 Society for Consumer Psychology. Published by
   Elsevier Inc. All rights reserved.},
