@article{ WOS:000355553700005,
Author = {Datta, Hannes and Foubert, Bram and Van Heerde, Harald J.},
Title = {The Challenge of Retaining Customers Acquired with Free Trials},
Journal = {JOURNAL OF MARKETING RESEARCH},
Year = {2015},
Volume = {52},
Number = {2},
Pages = {217-234},
Month = {APR},
Abstract = {Many service firms acquire customers by offering free-trial promotions.
   However, a crucial challenge is to retain the customers acquired with
   these free trials. To address this challenge, firms need to understand
   how free-trial customers differ from regular customers in terms of their
   decisions to retain the service. This article conceptualizes how
   marketing communication and usage behavior drive customers' retention
   decisions and develops hypotheses about the impact of free-trial
   acquisition on this process. To test the hypotheses, the authors model a
   customer's retention and usage decisions, distinguishing usage of a
   flat-rate service and usage of a pay-per-use service. The model allows
   for unobserved heterogeneity and corrects for selection effects and
   endogeneity. Using household panel data from a digital television
   service, the authors find systematic behavioral differences that cause
   the average customer lifetime value of free-trial customers to be 59\%
   lower than that of regular customers. However, free-trial customers are
   more responsive to marketing communication and usage rates, which offers
   opportunities to target marketing efforts and enhance retention rates,
   customer lifetime value, and customer equity.},
