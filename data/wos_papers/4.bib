@article{ WOS:000359178800002,
Author = {Plassmann, Hilke and Venkatraman, Vinod and Huettel, Scott and Yoon,
   Carolyn},
Title = {Consumer Neuroscience: Applications, Challenges, and Possible Solutions},
Journal = {JOURNAL OF MARKETING RESEARCH},
