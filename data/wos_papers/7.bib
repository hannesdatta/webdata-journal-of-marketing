@article{ WOS:000276120700003,
Author = {Nijs, Vincent and Misra, Kanishka and Anderson, Eric T. and Hansen,
   Karsten and Krishnamurthi, Lakshman},
Title = {Channel Pass-Through of Trade Promotions},
