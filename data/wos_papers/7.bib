@article{ WOS:000283027200008,
Author = {Lam, Son K. and Ahearne, Michael and Hu, Ye and Schillewaert, Niels},
Title = {Resistance to Brand Switching When a Radically New Brand Is Introduced:
   A Social Identity Theory Perspective},
Journal = {JOURNAL OF MARKETING},
