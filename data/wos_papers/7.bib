@article{ WOS:000271426000001,
Author = {Andrade, Eduardo B. and Ho, Teck-Hua},
Title = {Gaming Emotions in Social Interactions},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2009},
Volume = {36},
Number = {4},
Pages = {539-552},
Month = {DEC},
Abstract = {One's own emotions may influence someone else's behavior in a social
   interaction. If one believes this, she or he has an incentive to game
   emotions-to strategically modify the expression of a current emotional
   state-in an attempt to influence her or his counterpart. In a series of
   three experiments, this article investigates the extent to which people
   (1) misrepresent a current emotional state, (2) willfully acknowledge
   their strategic actions, (3) choose to game emotions over nonemotional
   information, and (4) improve their financial well-being from emotion
   gaming.},
