@article{ WOS:000273302200005,
Author = {Chaplin, Lan Nguyen and Lowrey, Tina M.},
Title = {The Development of Consumer-Based Consumption Constellations in Children},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2010},
Volume = {36},
Number = {5},
Pages = {757-777},
Month = {FEB},
Abstract = {Three studies using multiple methodologies investigated the development
   of consumer-based consumption constellations in children, finding an
   increasing linear age trend in the number of products and brands
   children use to form constellations, the degree to which these elements
   display symbolic complementarity, and the accessibility of
   constellations in memory. However, by early adolescence, as stereotypes
