@article{ WOS:000282749800007,
Author = {Liu, Ying and Ram, Sudha and Lusch, Robert F. and Brusco, Michael},
Title = {Multicriterion Market Segmentation: A New Model, Implementation, and
   Evaluation},
Journal = {MARKETING SCIENCE},
Year = {2010},
Volume = {29},
Number = {5},
Pages = {880-894},
Month = {SEP-OCT},
Abstract = {Market segmentation is inherently a multicriterion problem even though
   it has often been modeled as a single-criterion problem in the
   traditional marketing literature and in practice. This paper discusses
   the multicriterion nature of market segmentation and develops a new
   mathematical model that addresses this issue. A new method for market
   segmentation based on multiobjective evolutionary algorithms, called
   MMSEA, is developed. It complements existing segmentation methods by
   optimizing multiple objectives simultaneously, searching for globally
   optimal solutions, and approximating a set of Pareto-optimal solutions.
   We have applied and evaluated this method in two empirical studies for
   two firms from distinct industries: descriptive segmentation of the cell
   phone service market from a dual-value creation perspective and
   predictive segmentation of retail customers based on profit and customer
   sociodemographic attributes. The results provide decision makers with
   compelling alternatives and enhanced flexibility currently missing in
   existing market segmentation methods.},
Publisher = {INFORMS},
Address = {5521 RESEARCH PARK DR, SUITE 200, CATONSVILLE, MD 21228 USA},
Type = {Article},
Language = {English},
Affiliation = {Liu, Y (Corresponding Author), Calif State Univ Long Beach, Coll Business Adm, Long Beach, CA 90840 USA.
   Liu, Ying, Calif State Univ Long Beach, Coll Business Adm, Long Beach, CA 90840 USA.
   Ram, Sudha; Lusch, Robert F., Univ Arizona, Eller Coll Management, Tucson, AZ 85721 USA.
