@article{ WOS:000282486500008,
Author = {Venkatesh, Alladi and Joy, Annamma and Sherry, Jr., John F. and
   Deschenes, Jonathan},
Title = {The aesthetics of luxury fashion, body and identify formation},
Journal = {JOURNAL OF CONSUMER PSYCHOLOGY},
Year = {2010},
Volume = {20},
Number = {4, SI},
Pages = {459-470},
Month = {OCT},
Abstract = {In this paper we theorize and empirically investigate how female
   consumers' attitudes and preferences relating to bodily appearance are
   linked to their perceptions of the aesthetics of fashion. Our
   theoretical work is informed by three streams of research: aesthetics of
   production, aesthetics of reception and aesthetic labor. These three
   converge to illuminate our study. Using the ZMET technique, we uncover
   four themes: fashion as wearable art, body and self-identity, bodily
   appearance and high fashion brands, and aesthetic labor through fashion.
   Our focus on the aesthetics of fashion and identity formation provides a
   segue into the broader discussion of the growing importance of
   aesthetics in understanding consumer behavior. (C) 2010 Society for
   Consumer Psychology. Published by Elsevier Inc. All rights reserved.},
