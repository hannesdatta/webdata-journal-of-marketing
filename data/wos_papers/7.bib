@article{ WOS:000278743500004,
Author = {Grewal, Rajdeep and Chakravarty, Anindita and Saini, Amit},
Title = {Governance Mechanisms in Business-to-Business Electronic Markets},
Journal = {JOURNAL OF MARKETING},
Year = {2010},
Volume = {74},
