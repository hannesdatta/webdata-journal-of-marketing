@article{ WOS:000286979300002,
Author = {Danaher, Peter J. and Smith, Michael S.},
Title = {Modeling Multivariate Distributions Using Copulas: Applications in
   Marketing},
Journal = {MARKETING SCIENCE},
Year = {2011},
Volume = {30},
