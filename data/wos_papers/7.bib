@article{ WOS:000274194200010,
Author = {Kornish, Laura J. and Li, Qiuping},
Title = {Optimal Referral Bonuses with Asymmetric Information: Firm-Offered and
   Interpersonal Incentives},
Journal = {MARKETING SCIENCE},
