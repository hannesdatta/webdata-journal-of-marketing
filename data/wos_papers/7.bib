@article{ WOS:000277673000003,
Author = {Markman, Arthur B. and Loewenstein, Jeffrey},
Title = {Structural comparison and consumer choice},
Journal = {JOURNAL OF CONSUMER PSYCHOLOGY},
Year = {2010},
Volume = {20},
Number = {2},
Pages = {126-137},
Month = {APR},
Abstract = {Psychological research has demonstrated important parallels between the
   structural alignment process involved in judgments of similarity and the
   processes that are involved in analogical reasoning. Gradually, this
   work on comparisons was applied to consumer choice. In this paper, we
   review the influence of structural alignment on choices among a set of
   options. Then, we discuss extensions of this work to comparative
   advertising and to the role of analogy in marketing communication.
   Finally, we discuss important extensions to structural alignment
   suggested by this work such as the key role that affect plays in
   comparisons made during the choice process. (C) 2010 Society for
   Consumer Psychology. Published by Elsevier Inc. All rights reserved.},
