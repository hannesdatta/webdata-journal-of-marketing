@article{ WOS:000277947300006,
Author = {Ahearne, Michael and Mackenzie, Scott B. and Podsakoff, Philip M. and
   Mathieu, John E. and Lam, Son K.},
Title = {The Role of Consensus in Sales Team Performance},
Journal = {JOURNAL OF MARKETING RESEARCH},
Year = {2010},
Volume = {47},
Number = {3},
Pages = {458-469},
Month = {JUN},
Abstract = {Although team-based selling is highly prominent in practice, research on
   the drivers of its effectiveness is sparse. Drawing from the literature
   on climate consensus, the authors propose that in addition to leadership
   and team factors, team consensus plays a critical role in boosting sales
   team effectiveness. Using survey and archival data from a sample of 185
   pharmaceutical sales teams, the authors find that high team consensus
   regarding team-level leadership empowerment behaviors (LEBs) and team
   interpersonal climate quality enhances team potency given high LEBs but
   weakens team potency given low LEBs. In turn, team potency translates
   into sales team performance through both extra-role (team helping
   behavior) and in-role (team effort) behavior. The authors discuss the
   implications of these findings.},
