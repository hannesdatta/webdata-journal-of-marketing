@article{ WOS:000272636500005,
Author = {Hagtvedt, Henrik and Patrick, Vanessa M.},
Title = {The broad embrace of luxury: Hedonic potential as a driver of brand
   extendibility},
Journal = {JOURNAL OF CONSUMER PSYCHOLOGY},
Year = {2009},
Volume = {19},
Number = {4},
Pages = {608-618},
Month = {OCT},
Abstract = {This research proposes a feelings-based account of brand extension
   evaluation and demonstrates that the promise of pleasure (hedonic
   potential) associated with luxury brands is a key driver of brand
   extendibility. In four studies, we contrast a luxury brand with a value
   brand. Both brand concepts lead to equally favorable brand evaluations,
   but the luxury brand concept results in more favorable brand extension
   evaluations due to the hedonic potential inherent in this concept.
   However, the luxury brand is shown to be sensitive to inconsistent brand
   cues, leading to diminished hedonic potential and consequently decreased
   brand and brand extension evaluations. (C) 2009 Society for Consumer
   Psychology. Published by Elsevier Inc. All rights reserved.},
Publisher = {JOHN WILEY \& SONS LTD},
