@article{ WOS:000282749800008,
Author = {Dotson, Jeffrey P. and Allenby, Greg M.},
Title = {Investigating the Strategic Influence of Customer and Employee
   Satisfaction on Firm Financial Performance},
Journal = {MARKETING SCIENCE},
Year = {2010},
Volume = {29},
