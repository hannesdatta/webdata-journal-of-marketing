@article{ WOS:000281583600005,
Author = {Chernev, Alexander and Boeckenholt, Ulf and Goodman, Joseph},
Title = {Commentary on Scheibehenne, Greifeneder, and Todd Choice Overload: Is
   There Anything to It?},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2010},
Volume = {37},
Number = {3},
Pages = {426-428},
Month = {OCT},
Abstract = {Can there ever be too many options? We argue that because choice
   overload has multiple antecedents, simply searching for a main effect
   across all conditions and a single ``sufficient{''} condition that is
   likely to solely predict this effect is not informative. Moreover,
   because prior research has documented multiple instances in which an
   abundance of options leads to choice overload, the interesting question
   is not whether choice overload occurs but when it occurs. The answer to
   this question is unlikely to stem from testing for the presence of a
   single main effect using the traditional meta-analytic approach.
   Instead, research would benefit from a theory-based meta-analysis that
   tests the validity of a conceptual model of choice overload capturing
   the underlying psychological processes.},
