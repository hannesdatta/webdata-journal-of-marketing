@article{ WOS:000277673000008,
Author = {Chaplin, Lan Nguyen and John, Deborah Roedder},
Title = {Interpersonal influences on adolescent materialism: A new look at the
   role of parents and peers},
Journal = {JOURNAL OF CONSUMER PSYCHOLOGY},
Year = {2010},
Volume = {20},
Number = {2},
Pages = {176-184},
Month = {APR},
Abstract = {What causes adolescents to be materialistic? Prior research shows
   parents and peers are an important influence. Researchers have viewed
   parents and peers as socialization agents that transmit consumption
   attitudes, goals, and motives to adolescents. We take a different
   approach, viewing parents and peers as important sources of emotional
   support and psychological well-being, which increase self-esteem in
   adolescents. Supportive parents and peers boost adolescents'
   self-esteem, which decreases their need to turn to material goods to
   develop positive self-perceptions. In a study with 12-18 year-olds, we
   find support for our view that self-esteem mediates the relationship
   between parent/peer influence and adolescent materialism. (C) 2010
   Society for Consumer Psychology. Published by Elsevier Inc. All rights
   reserved.},
