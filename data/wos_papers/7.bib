@article{ WOS:000287335200002,
Author = {Bos, Maarten W. and Dijksterhuis, Ap and van Baaren, Rick B.},
Title = {The benefits of ``sleeping on things{''}: Unconscious thought leads to
   automatic weighting},
Journal = {JOURNAL OF CONSUMER PSYCHOLOGY},
Year = {2011},
Volume = {21},
Number = {1, SI},
Pages = {4-8},
Month = {JAN},
Abstract = {We tested and confirmed the hypothesis that unconscious thought leads to
   an automatic weighting process whereby important decision attributes
   receive more weight, and unimportant decision attributes receive less
   weight. In three experiments, participants chose between cars with few
   important positive attributes and many unimportant negative attributes
   ({''}Quality cars{''}), and cars with many unimportant positive
   attributes and few important negative attributes ({''}Frequency
   cars{''}). In all experiments, unconscious thinkers showed a stronger
   preference for Quality cars than immediate decision makers, showing that
   unconscious thought indeed evokes an automatic weighting process. An
   alternative explanation is refuted and implications are discussed. (C)
   2010 Society for Consumer Psychology. Published by Elsevier Inc. All
   rights reserved.},
Publisher = {JOHN WILEY \& SONS LTD},
Address = {THE ATRIUM, SOUTHERN GATE, CHICHESTER PO19 8SQ, W SUSSEX, ENGLAND},
