@article{ WOS:000296317200012,
Author = {Galak, Jeff and Small, Deborah and Stephen, Andrew T.},
Title = {Microfinance Decision Making: A Field Study of Prosocial Lending},
Journal = {JOURNAL OF MARKETING RESEARCH},
Year = {2011},
Volume = {48},
Number = {SI},
Pages = {S130-S137},
Abstract = {Microfinancing, or small uncollateralized loans to entrepreneurs in the
   developing world, has recently emerged as a leading contender to cure
   world poverty. Our research investigates the characteristics of
   borrowers that engender lending through Kiva, a popular organization
   that connects individual lenders to borrowers through online
   microfinance. Lenders favor individual borrowers over groups or
   consortia of borrowers, a pattern consistent with the identifiable
   victim effect. They also favor borrowers that are socially proximate to
   themselves. Across three dimensions of social distance (gender,
   occupation, and first name initial), lenders prefer to give to those who
   are more like themselves.},
