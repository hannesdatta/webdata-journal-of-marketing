@article{ WOS:000274103900007,
Author = {van Nierop, Erjen and Bronnenberg, Bart and Paap, Richard and Wedel,
   Michel and Franses, Philip Hans},
Title = {Retrieving Unobserved Consideration Sets from Household Panel Data},
Journal = {JOURNAL OF MARKETING RESEARCH},
Year = {2010},
Volume = {47},
Number = {1},
Pages = {63-74},
Month = {FEB},
Abstract = {The authors propose a new model to capture unobserved consideration from
   discrete choice data. This approach allows for unobserved dependence in
   consideration among brands, easily copes with many brands, and
   accommodates different effects of the marketing mix on consideration and
   choice as well as unobserved consumer heterogeneity in both processes.
   An important goal of this study is to establish the validity of the
   existing practice to infer consideration sets from observed choices in
   panel data. The authors show with experimental data that underlying
   consideration sets can be reliably retrieved from choice data alone and
   that consideration is positively affected by display and shelf space.
   Next, the model is applied to Information Resources Inc. panel data. The
   findings suggest that promotion effects are larger when they are
   included in the consideration stage of the two-stage model than in a
   single-stage model. The authors also find that consideration covaries
   across brands and that this covariation is mainly driven by unobserved
   consumer heterogeneity. Finally, the authors show the implications of
   the model for promotion planning relative to a more standard model of
   choice.},
