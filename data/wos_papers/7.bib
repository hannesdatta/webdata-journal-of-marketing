@article{ WOS:000273302200004,
Author = {Elder, Ryan S. and Krishna, Aradhna},
Title = {The Effects of Advertising Copy on Sensory Thoughts and Perceived Taste},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2010},
Volume = {36},
Number = {5},
Pages = {748-756},
Month = {FEB},
Abstract = {We propose that advertisement (ad) content for food products can affect
   taste perception by affecting sensory cognitions. Specifically, we show
   that multisensory ads result in higher taste perceptions than ads
   focusing on taste alone, with this result being mediated by the excess
   of positive over negative sensory thoughts. Since the ad effect is
   thoughts-driven or cognitive, restricting cognitive resources (imposing
   cognitive load) attenuates the enhancing effect of the multiple-sense
   ad. Our results are exhibited across three experiments and have many
   implications for cognition and sensory perception research within
   consumer behavior, as well as several practical implications.},
