@article{ WOS:000286979300011,
Author = {Chandukala, Sandeep R. and Dotson, Jeffrey P. and Brazell, Jeff D. and
   Allenby, Greg M.},
Title = {Bayesian Analysis of Hierarchical Effects},
Journal = {MARKETING SCIENCE},
Year = {2011},
Volume = {30},
