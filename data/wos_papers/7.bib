@article{ WOS:000282110400014,
Author = {Haws, Kelly L. and Dholakia, Utpal M. and Bearden, William O.},
Title = {An Assessment of Chronic Regulatory Focus Measures},
Journal = {JOURNAL OF MARKETING RESEARCH},
Year = {2010},
Volume = {47},
Number = {5},
Pages = {967-982},
Month = {OCT},
Abstract = {Prior consumer research has demonstrated the ability of promotion and
   prevention regulatory orientations to moderate a variety of consumer and
   marketing phenomena but also has used several different methods to
   measure chronic regulatory focus. This article assesses five chronic
   regulatory focus measures using the criteria of theoretical coverage,
   internal consistency, homogeneity, stability, and predictive ability.
   The results reveal a lack of convergence among the measures and
   variation in their performance along these criteria. The authors provide
   specific guidance for choosing a particular measure in regulatory focus
   research and suggest a composite measure.},
