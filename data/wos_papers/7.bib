@article{ WOS:000272636500010,
Author = {Kramer, Thomas and Lau-Gesk, Loraine and Chiu, Chi-yue},
Title = {The interactive effects of duality expertise and coping frames on
   responses to ambivalent messages},
Journal = {JOURNAL OF CONSUMER PSYCHOLOGY},
Year = {2009},
Volume = {19},
Number = {4},
Pages = {661-672},
Month = {OCT},
Abstract = {We examine the interactive effects of biculturals' duality expertise and
   externally provided coping resources on attitudinal responses to
   ambivalence. Three studies reveal that ambivalence is associated with
   greater discomfort for biculturals more (vs. less) conflicted about
   their cultural duality and with limited exposure to accessing their two
   cultural knowledge systems simultaneously. Among biculturals with lower
   duality expertise, coping frames lower their negative evaluation of
   messages that elicit ambivalence because coping frames help these
   biculturals resolve the discomfort associated with ambivalence.
   Provision of coping frames does not impact attitudes of biculturals with
   greater levels of duality expertise. (C) 2009 Society for Consumer
   Psychology. Published by Elsevier Inc. All rights reserved.},
