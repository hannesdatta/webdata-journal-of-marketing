@article{ WOS:000271442800018,
Author = {Karniouchina, Ekaterina V. and Moore, William L. and Cooney, Kevin J.},
Title = {Impact of Mach Money Stock Recommendations: Merging Financial and
   Marketing Perspectives},
Journal = {JOURNAL OF MARKETING},
