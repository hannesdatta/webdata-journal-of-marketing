@article{ WOS:000271426000007,
Author = {Chen, Tao and Kalra, Ajay and Sun, Baohong},
Title = {Why Do Consumers Buy Extended Service Contracts?},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2009},
Volume = {36},
Number = {4},
Pages = {611-623},
Month = {DEC},
Abstract = {We examine purchases of extended service contracts, which are
   essentially insurance products, for electronic products in a retail
   setting. The primary insurance purchase determinants are perceived
   probability of loss, extent of loss, risk aversion, and amount of
   insurance premium. We examine how product characteristics
   (hedonic/utilitarian, manufacturer's warranty) and retailer actions (
   promotions, feature advertising) influence the purchase of extended
   service contracts. We also investigate the impact of consumer
   characteristics ( income, gender, and prior usage) on these insurance
   purchase determinants. To test the predictions, we use revealed
   preferences from panel data of electronic purchases across several
   product categories.},
