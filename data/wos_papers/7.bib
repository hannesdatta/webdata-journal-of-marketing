@article{ WOS:000283664800011,
Author = {Fedorikhin, Alexander and Patrick, Vanessa M.},
Title = {Positive Mood and Resistance to Temptation: The Interfering Influence of
   Elevated Arousal},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2010},
Volume = {37},
Number = {4},
Pages = {698-711},
Month = {DEC},
Abstract = {We investigate the interfering influence of elevated arousal on the
   impact of positive mood on resistance to temptation. Three studies
   demonstrate that when a temptation activates long-term health goals,
   baseline positive mood facilitates resistance to temptation in (1) the
   choice between two snack items, one of which is more unhealthy, sinful,
   and hard to resist (M\&Ms) than the other (grapes) and (2) the
   monitoring of consumption when the sinful option is chosen. However,
   this influence is attenuated when positive mood is accompanied by
   elevated arousal. We demonstrate that the cognitive depletion that
   accompanies elevated arousal interferes with the self-regulatory focus
   of positive mood, decreasing resistance to temptation.},
Publisher = {OXFORD UNIV PRESS INC},
