@article{ WOS:000226189500008,
Author = {Prabhu, JC and Chandy, RK and Ellis, ME},
Title = {The impact of acquisitions on innovation: Poison pill, placebo, or
   tonic?},
Journal = {JOURNAL OF MARKETING},
Year = {2005},
Volume = {69},
Number = {1},
Pages = {114-130},
Month = {JAN},
Abstract = {Do acquisitions increase, decrease, or have no effect on innovation? The
   empirical research on this question suggests that acquisitions may hurt
   innovation; that is, they may be a ``poison pill{''} for innovation. The
   authors present an alternative view. For firms that first engage in
   internal knowledge development, the knowledge-based view the authors
   present suggests that acquisitions can help innovation; that is, they
   can be a tonic for innovation. Analysis of cross-sectional, time-series
   data on a sample of pharmaceutical firms during 1988-97 provides
   evidence to support the thesis.},
