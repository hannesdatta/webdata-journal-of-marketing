@article{ WOS:000225417400001,
Author = {Priester, JR and Dholakia, UM and Fleming, MA},
Title = {When and why the background contrast effect emerges: Thought engenders
   meaning by influencing the perception of applicability},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2004},
Volume = {31},
Number = {3},
Pages = {491-501},
Month = {DEC},
Note = {33rd Annual Conference of the Association-for-Consumer-Research,
   ATLANTA, GA, OCT 16-20, 2002},
Organization = {Assoc Consumer Res},
Abstract = {The Background Contrast Effect occurs when the trade-off value between
   attributes in a first choice influences subsequent choice. We explore
   the role of decision-making thoughtfulness in seeking to understand when
   and why this effect occurs. Experiments 1 and 2 provide evidence that
   the Background Contrast Effect emerges more under thoughtful than under
   nonthoughtful conditions. Experiment 3 reveals that thought influences
   perceptions of applicability of the trade-off values in the first choice
   to the second. Experiment 4 demonstrates that when the information from
   the first choice is manipulated to appear inapplicable, the Background
   Contrast Effect is reversed under thoughtful conditions. This research
   highlights the role of thought on perception of applicability as a
   process underlying when and why context is used to generate meaning in
   choice situations.},
