@article{ WOS:000226965600007,
Author = {Okada, EM},
Title = {Justification effects on consumer choice of hedonic and utifitarian
   goods},
Journal = {JOURNAL OF MARKETING RESEARCH},
Year = {2005},
Volume = {42},
Number = {1},
Pages = {43-53},
Month = {FEB},
Abstract = {People want to have fun, and they are more likely to have fun if the
   situation allows them to justify it. This research studies how people's
   need for justifying hedonic consumption drives two choice patterns that
   are observed in typical purchase contexts. First, relative preferences
   between hedonic and utilitarian alternatives can reverse, depending on
   how the immediate purchase situation presents itself. A hedonic
   alternative tends to be rated more highly than a comparable utilitarian
   alternative when each is presented singly, but the utilitarian
   alternative tends to be chosen over the hedonic alternative when the two
   are presented jointly. Second, people have preferences for expending
   different combinations of time (effort) and money for acquiring hedonic
   versus utilitarian items. They are willing to pay more in time for
   hedonic goods and more in money for utilitarian goods. The author
   explores the topic through a combination of four experiments and field
   studies.},
