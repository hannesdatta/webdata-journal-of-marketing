@article{ WOS:000236224600012,
Author = {Passyn, K and Sujan, M},
Title = {Self-accountability emotions and fear appeals: Motivating behavior},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2006},
Volume = {32},
Number = {4},
Pages = {583-589},
Month = {MAR},
Abstract = {We examine the role of high self-accountability emotions in enhancing
   compliance with fear appeals. In two field experiments, we find that
   relative to straight fear appeals (negative) or adding hope (positive),
   which ascribes low accountability to the self, action-facilitative
   coping, intentions, and behaviors (using sunscreen, eating high fiber
   foods) are enhanced by adding guilt, regret (both negative), or
   challenge (positive), all of which induce feelings of high
   self-accountability. In addition, we find that cold perceptions of high
   accountability are necessary but insufficient to influence actual
   behaviors and that the corresponding emotion is an essential driver of
   behaviors.},
