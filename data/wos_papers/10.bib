@article{ WOS:000235464300008,
Author = {LeBoeuf, RA},
Title = {Discount rates for time versus dates: The sensitivity of discounting to
   time-interval description},
Journal = {JOURNAL OF MARKETING RESEARCH},
Year = {2006},
Volume = {43},
Number = {1},
Pages = {59-72},
Month = {FEB},
Abstract = {Six experiments examine the impact of time-interval descriptions on
   consumers' discount rates. Consumers exhibit more discounting (e.g.,
   they demand more money to delay income) when delay intervals are
   described by extents of time than when delay intervals are described by
   dates. This pattern holds in various contexts, including gains and
   losses, and is robust to response-mode changes. Findings further
   indicate that this effect may arise because, compared with date
   descriptions, extent descriptions enhance perceptions of interval
   length. The author discusses other potential mechanisms and
   implications.},
