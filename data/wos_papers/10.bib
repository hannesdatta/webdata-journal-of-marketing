@article{ WOS:000230239800004,
Author = {Olson, EM and Slater, SF and Hult, GTM},
Title = {The performance implications of fit among business strategy, marketing
   organization structure, and strategic behavior},
Journal = {JOURNAL OF MARKETING},
Year = {2005},
Volume = {69},
Number = {3},
Pages = {49-65},
Month = {JUL},
Abstract = {Adopting a contingency perspective, the authors present and test a
   fit-as-mode ration model that posits that overall firm performance is
   influenced by how well the marketing organization's structural
   characteristics (i.e., formalization, centralization, and
   specialization) and strategic behavioral emphases (i.e., customer,
   competitor, innovation, and cost control) complement alternative
   business strategies (i.e., prospector, analyzer, low-cost defender, and
   differentiated defender). Responses from 228 senior marketing managers
   provide support for the model and demonstrate that each strategy type
   requires different combinations of marketing organization structures and
   strategic behaviors for success.},
Publisher = {SAGE PUBLICATIONS INC},
