@article{ WOS:000235464300001,
Author = {Avnet, T and Higgins, ET},
Title = {How regulatory fit affects value in consumer choices and opinions},
Journal = {JOURNAL OF MARKETING RESEARCH},
Year = {2006},
Volume = {43},
Number = {1},
Pages = {1-10},
Month = {FEB},
Abstract = {Regulatory fit occurs when the manner of peoples' engagement in an
   activity sustains their current goal orientation or concerns with that
   activity. It is proposed that regulatory fit changes the significance of
   consumers' reactions to something, including the perceived monetary
   value of a choice they have made or the persuasiveness of a message they
   have received. When there is a fit, people engage more strongly in and
   ``feel right{''} -about what they are doing, and subsequent evaluative
   reactions (positive or negative) can be intensified by this fit
   experience. The fit experience is shown to influence the strength of
   value experiences independent of hedonic experiences. The authors
   discuss how the fit effect on value is distinct from other factors that
   affect value, such as relevancy, matching, hedonic mood, and arousal.
   Using prior research on this topic, this article summarizes the current
   state of knowledge about how fit influences value and offers new ideas
   for further research.},
