@article{ WOS:000239705800010,
Author = {Rotte, Kristin and Chandrashekaran, Murali and Tax, Stephen S. and
   Grewal, Rajdeep},
Title = {Forgiven but not forgotten: Covert uncertainty in overt responses and
   the paradox of defection-despite-trust},
Journal = {JOURNAL OF CONSUMER PSYCHOLOGY},
Year = {2006},
Volume = {16},
Number = {3},
Pages = {283-294},
Abstract = {Despite the widespread belief that trust is a critical determinant of
   loyalty, empirical and anecdotal evidence calls into question the
   real-world robustness of the trust-loyalty link. An important reason for
   the fuzzy nature of the trust-loyalty link may be the fuzzy nature of
   trust itself. That is, stated trust judgments embody both a magnitude
   dimension (i.e., the position along a favorable-unfavorable continuum)
   and an uncertainty dimension (i.e., the lack of conviction with which
   the judgment is held). We investigated this possibility using data
   pertaining to consumers' reactions to a service failure and the
   provider's success in responding to their complaints. We found that the
   interplay between dissatisfaction with the complaint handling and past
   experience simultaneously influences trust magnitude and trust
   uncertainty. However, these two dimensions of trust are shaped by
   different underlying processes. Finally, uncertainty dampens the impact
   of stated trust on loyalty.},
