@article{ WOS:000236894600002,
Author = {Doyle, JR and Bottomley, PA},
Title = {Dressed for the occasion: Font-product congruity in the perception of
   logotype},
Journal = {JOURNAL OF CONSUMER PSYCHOLOGY},
Year = {2006},
Volume = {16},
Number = {2},
Pages = {112-123},
Abstract = {Styles of lettering (i.e., fonts) can differ in their appropriateness
   for describing certain types of brands and products. We use the Osgood
   dimensions of evaluation, potency, and activity to measure the
   connotative meaning of fonts and product categories. Judgments of the
   appropriateness of a font for a product partly depend on the consistency
   between the connotative meaning of the font and that of the product
   along dimensions of potency and activity. However, its perceived
   appropriateness also depends on the connotative meaning of the font per
   se, independent of the type of product being described. These
   differences are also evident when participants choose a company to call
   on the basis of ads similar to those found in the Yellow Pages. Finally,
   the effect of the meaning that people assign to a font is similar to the
   effect of the meaning they assign to the product's name itself.},
