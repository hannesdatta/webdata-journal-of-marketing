@article{ WOS:000236224600008,
Author = {Spangenberg, ER and Sprott, DE},
Title = {Self-monitoring and susceptibility to the influence of self-prophecy},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2006},
Volume = {32},
Number = {4},
Pages = {550-556},
Month = {MAR},
Abstract = {Having people predict whether they will perform a socially normative
   behavior increases their probability of performing that target action.
   Recent empirical evidence supports a dissonance-based theoretical
   explanation for this self-prophecy effect. While the effect is robust,
   few boundary conditions have been identified. We report two experiments
   within the dissonance paradigm providing theory-relevant evidence for
   the moderating effects of self-monitoring on people's susceptibility to
   self-prophecy. In particular, low self-monitors are more likely than
   high self-monitors to be influenced by self-prediction regarding
   normative behaviors. Implications for theory and the practice of social
   influence are provided.},
