@article{ WOS:000236894600005,
Author = {Suh, JC and Yi, YJ},
Title = {When brand attitudes affect the customer satisfaction-loyalty relation:
   The moderating role of product involvement},
Journal = {JOURNAL OF CONSUMER PSYCHOLOGY},
Year = {2006},
Volume = {16},
Number = {2},
Pages = {145-155},
Abstract = {Drawing on the attitude accessibility and stability theories, we
   investigate the moderating role of product involvement in the customer
   satisfaction-loyalty relation. Structural equation modeling shows that
   customer satisfaction has both direct and indirect effects on loyalty,
   whereas ad attitudes and corporate image have only indirect effects
   through their mediating influence on brand attitudes. Furthermore,
   product involvement decreases the direct effects of satisfaction on
   brand attitudes and loyalty, but it increases the indirect effects of ad
   attitudes and corporate image.},
Publisher = {JOHN WILEY \& SONS LTD},
