DOI = {10.1509/jmkr.42.2.183.62289},
ISSN = {0022-2437},
EISSN = {1547-7193},
Keywords-Plus = {CONSUMER EVALUATIONS; PRODUCT; CATEGORIZATION},
Research-Areas = {Business \& Economics},
