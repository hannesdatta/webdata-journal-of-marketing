@article{ WOS:000239705800003,
Author = {Vohs, Kathleen D.},
Title = {Self-regulatory resources power the reflective system: Evidence from
   five domains},
Journal = {JOURNAL OF CONSUMER PSYCHOLOGY},
Year = {2006},
Volume = {16},
Number = {3},
Pages = {217-223},
Abstract = {In this commentary, I focus on the deficiencies in the
   reflective-impulsive model (RIM) by Strack, Werth, and Deutsch (2006) in
   terms of understanding the mechanics of the reflective system. Strack et
   al. outlined the cognitive architecture of the consumer with the RIM but
   failed to specify how its most impressive feature, the reflective
   system, is powered. Drawing on the literature on self-regulation (as a
   reconceptualization of RIM incompatibility), I argue that
   self-regulatory resources drive the reflective system. Research from 5
   domains-overeating among dieters, impulsive spending, logical thinking,
   making choices, and subjective perceptions of duration-supports this
   hypothesis.},
