@article{ WOS:000397222500004,
Author = {Maciel, Andre F. and Wallendorf, Melanie},
Title = {Taste Engineering: An Extended Consumer Model of Cultural Competence
   Constitution},
Journal = {JOURNAL OF CONSUMER RESEARCH},
