@article{ WOS:000377348400007,
Author = {Huang, Dongling and Luo, Lan},
Title = {Consumer Preference Elicitation of Complex Products Using Fuzzy Support
   Vector Machine Active Learning},
Journal = {MARKETING SCIENCE},
