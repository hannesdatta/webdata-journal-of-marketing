@article{ WOS:000395374700012,
Author = {John, Leslie K. and Emrich, Oliver and Gupta, Sunil and Norton, Michael
   I.},
Title = {Does ``Liking{''} Lead to Loving? The Impact of Joining a Brand's Social
   Network on Marketing Outcomes},
Journal = {JOURNAL OF MARKETING RESEARCH},
Year = {2017},
Volume = {54},
Number = {1},
Pages = {144-155},
Month = {FEB},
Abstract = {Does ``liking{''} a brand on Facebook cause a person to view it more
   favorably? Or is ``liking{''} simply a symptom of being fond of a brand?
   The authors disentangle these possibilities and find evidence for the
   latter: brand attitudes and purchasing are predicted by consumers'
   preexisting fondness for brands, and these are the same regardless of
   when and whether consumers ``like{''} brands on social media. In
   addition, we explore possible second-order effects by examining whether
   ``liking{''} brands might cause consumers' friends to view that brand
   more favorably. When consumers see that a friend has ``liked{''} a
   brand, they are less likely to buy the brand relative to when they learn
   that a friend genuinely likes the brand in the offline sense, which is a
   more meaningful social endorsement. Taken together, five experiments and
   two meta-analyses (N > 14,000) suggest that turning ``liking{''} into
   improved brand attitudes and increased purchasing by consumers and their
   friends may require more than just the click of a button.},
