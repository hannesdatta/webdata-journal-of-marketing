@article{ WOS:000369154000010,
Author = {Ching, Andrew T. and Clark, Robert and Horstmann, Ignatius and Lim,
   Hyunwoo},
Title = {The Effects of Publicity on Demand: The Case of Anti-Cholesterol Drugs},
Journal = {MARKETING SCIENCE},
Year = {2016},
