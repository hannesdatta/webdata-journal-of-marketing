@article{ WOS:000383692200008,
Author = {Chakravarty, Anindita and Grewal, Rajdeep},
Title = {Analyst Earning Forecasts and Advertising and R\&D Budgets: Role of
   Agency Theoretic Monitoring and Bonding Costs},
Journal = {JOURNAL OF MARKETING RESEARCH},
