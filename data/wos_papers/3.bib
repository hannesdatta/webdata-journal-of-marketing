@article{ WOS:000378149500001,
Author = {Hewett, Kelly and Rand, William and Rust, Roland T. and van Heerde,
   Harald J.},
Title = {Brand Buzz in the Echoverse},
Journal = {JOURNAL OF MARKETING},
