@article{ WOS:000404694900003,
Author = {Arens, Zachary G. and Hamilton, Rebecca W.},
Title = {The rebound of the forgone alternative},
Journal = {JOURNAL OF CONSUMER PSYCHOLOGY},
Year = {2017},
Volume = {27},
Number = {3},
Pages = {318-332},
Month = {JUL},
Abstract = {Fifty years of cognitive dissonance research suggests that when
   consumers make a difficult choice, the alternative they forgo is
   devalued for an extended period of time, making it less likely to be
   chosen in the future. In a series of four studies, we show that
   completely consuming the chosen alternative moderates this effect. After
   the chosen alternative has been consumed, creating a sense of
   consumption closure, the attractiveness of forgone alternative rebounds
   to its original value. (C) 2017 Society for Consumer Psychology.
   Published by Elsevier Inc. All rights reserved.},
