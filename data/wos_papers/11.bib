@article{ WOS:000179717500006,
Author = {Nordhielm, CL},
Title = {The influence of level of processing on advertising repetition effects},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2002},
Volume = {29},
Number = {3},
Pages = {371-382},
Month = {DEC},
Abstract = {This research examines whether or not repetition of features of a
   stimulus are subject to wear-out effects that have until now only been
   tested for the stimulus as a whole. When consumers process features in
   either a shallower or deeper manner, the level of processing performed
   dictates the effect of repeated feature exposure on their judgments.
   When repeated exposures to features are processed in a shallower
   fashion, there is an enhancement in evaluations with no subsequent
   downturn, whereas repeated exposure to features that are processed more
   deeply results in evaluations that exhibit the classic inverted U-shaped
   pattern.},
