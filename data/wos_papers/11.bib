@article{ WOS:000178782300004,
Author = {Zou, SM and Cavusgil, ST},
Title = {The GMS: A broad conceptualization of global marketing strategy and its
   effect on firm performance},
Journal = {JOURNAL OF MARKETING},
Year = {2002},
Volume = {66},
Number = {4},
Pages = {40-56},
Month = {OCT},
Abstract = {Despite the strong interest in global marketing, there is no consensus
   in the literature about what constitutes a global marketing strategy and
   whether it affects a firm's global market performance. The authors
   develop a broad conceptualization of global marketing strategy, the GMS,
   that integrates three major perspectives-namely, the standardization,
   configuration-coordination, and integration perspectives-of global
   marketing strategy. They also develop a conceptual model that links the
   GMS to a firm's global market performance. On the basis of a survey of
   business units competing in global industries, the authors find support
   for the broad GMS perspective and the fundamental relationship between
   the GMS and firms' global market performance. The authors also discuss
   theoretical and managerial implications of their findings.},
