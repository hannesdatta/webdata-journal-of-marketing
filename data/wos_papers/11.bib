@article{ WOS:000224129300001,
Author = {Brooks, CM and Kaufmann, PJ and Lichtenstein, DR},
Title = {Travel configuration on consumer trip-chained store choice},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2004},
Volume = {31},
Number = {2},
Pages = {241-248},
Month = {SEP},
Abstract = {We predict that in evaluating alternative equidistant trip chains (i.e.,
   the combining of multiple destinations into a single outing), consumers
   will choose trip chains where destinations are more clustered (i.e.,
   closer to each other) and further from the origin over equidistant trip
   chains where destinations are less clustered but closer to the origin.
   This prediction is based on the assumptions of diminishing marginal
   sensitivity and reference-point dependence postulated by
   reference-dependent theory. Results of the first experiment provide
   support for this prediction. Results of the second experiment provide
   evidence that trip-chain configuration affects route choice via both
   facets of reference-dependent theory.},
