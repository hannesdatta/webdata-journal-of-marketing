@article{ WOS:000224129300015,
Author = {Burroughs, JE and Mick, DG},
Title = {Exploring antecedents and consequences of consumer creativity in a
   problem-solving context},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2004},
Volume = {31},
Number = {2},
Pages = {402-411},
Month = {SEP},
Abstract = {Creativity is an underresearched topic in consumer behavior, yet
   integral in many instances of consumer problem solving. Two experiments
   were conducted to investigate antecedents and consequences of creativity
   in a consumption context. The results indicate that both situational
   factors (i.e., time constraints, situational involvement) and person
   factors (i.e., locus of control, metaphoric thinking ability) affect
   creative consumption and that some of these variables have interactive
   influences. The results also show that acting creatively enhances
   positive affect.},
