@article{ WOS:000185215800008,
Author = {Joy, A and Sherry, JF},
Title = {Speaking of art as embodied imagination: A multisensory approach to
   understanding aesthetic experience},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2003},
Volume = {30},
Number = {2},
Pages = {259-282},
Month = {SEP},
Abstract = {This article focuses on somatic experience-not just the process of
   thinking bodily but how the body informs the logic of thinking about
   art. We examine the links between embodiment, movement, and multisensory
   experience insofar as they help to elucidate the contours of art
   appreciation in a museum. We argue that embodiment can be identified at
   two levels: the phenomenological and the cognitive unconscious. At the
   first level, individuals are conscious of their feelings and actions
   while, at the second level, sensorimotor and other bodily oriented
   inference mechanisms inform their processes of abstract thought and
   reasoning. We analyze the consumption stories of 30 museum goers in
   order to understand how people move through museum spaces and feel,
   touch, hear, smell, and taste art. Further, through an analysis of
   metaphors and the use of conceptual blending, we tap into the
   participants' unconscious minds, gleaning important embodiment processes
   that shape their reasoning.},
