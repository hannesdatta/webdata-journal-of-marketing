@article{ WOS:000187223500008,
Author = {McCabe, DB and Nowlis, SM},
Title = {The effect of examining actual products or product descriptions on
   consumer preference},
Journal = {JOURNAL OF CONSUMER PSYCHOLOGY},
Year = {2003},
Volume = {13},
Number = {4},
Pages = {431-439},
Note = {INFORMS 2000 Meeting, SALT LAKE CITY, UTAH, MAY   07, 2000},
Organization = {INFORMS},
Abstract = {Many consumers purchase products in stores, where they can physically
   examine and touch the items. In addition, consumers shop for products
   online or through direct mail, where they cannot physically examine and
   touch the merchandise. Building on an analysis of perceptual mechanisms
   involved in the sense of touch, we find that products with primarily
   material properties, such as clothing or carpeting, are more likely to
   be preferred in shopping environments that allow physical inspection
   than in those environments that do not. We also find that here is no
   difference in preference across the two environments for products with
   primarily geometric properties, such as packaged goods, for which vision
   is highly diagnostic. Furthermore, when the touch properties of a
   material product are verbally described, this reduces the difference in
   preference between the two environments.},
