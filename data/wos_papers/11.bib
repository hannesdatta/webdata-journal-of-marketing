@article{ WOS:000183354100010,
Author = {Folkes, VS and Patrick, VM},
Title = {The Positivity effect in perceptions of services: Seen one, seen them
   all?},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2003},
Volume = {30},
Number = {1},
Pages = {125-137},
Month = {JUN},
Abstract = {A series of studies show converging evidence of a positivity effect in
   consumers' inferences about service providers. When the consumer has
   little experience with a service, positive information about a single
   employee leads to inferences that the firm's other service providers are
   similarly positive to a greater extent than negative information leads
   to inferences that the firm's other service providers are similarly
   negative. Four studies were conducted that varied in the amount of
   information about the service provider, the firm, and the service. The
   positivity effect was supported despite differences across studies in
   methods as well as measures.},
