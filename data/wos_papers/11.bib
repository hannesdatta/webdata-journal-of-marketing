@article{ WOS:000176835600007,
Author = {Speier, C and Venkatesh, V},
Title = {The hidden minefields in the adoption of sales force automation
   technologies},
Journal = {JOURNAL OF MARKETING},
Year = {2002},
Volume = {66},
Number = {3},
Pages = {98-111},
Month = {JUL},
Abstract = {Sales force automation technologies are increasingly used to support
   customer relationship management strategies; however, commentary in the
   practitioner press suggests high failure rates. The authors use identity
   theory as a lens to better understand salesperson perceptions associated
   with technology rejection. They collected survey data from 454
   salespeople across two firms that had implemented sales force automation
   tools, The results indicate that immediately after training, salespeople
   had positive perceptions of the technology. However, six months after
   implementation, the technology had been widely rejected, and salesperson
   absenteeism and voluntary turnover had significantly increased. There
   were also significant decreases in perceptions of organizational
   commitment, job satisfaction, person-organization fit, and person-job
   fit across both firms. Finally, salespeople with stronger professional
   commitment indicated more negative job-related perceptions as experience
   with the technology increased.},
