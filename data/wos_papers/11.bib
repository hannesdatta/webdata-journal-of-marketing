@article{ WOS:000222514000008,
Author = {Aggarwal, P},
Title = {The effects of brand relationship norms on consumer attitudes and
   behavior},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2004},
Volume = {31},
Number = {1},
Pages = {87-101},
Month = {JUN},
Abstract = {The key premise underlying this work is that when consumers form
   relationships with brands they use norms of interpersonal relationships
   as a guide in their brand assessments. Two relationship types are
   examined: exchange relationships in which benefits are given to others
   to get something back and communal relationships in which benefits are
   given to show concern for other's needs. The conceptual model proposes
   that an adherence to or a violation of these relationship norms
   influences the appraisal of the specific marketing action and also the
   overall brand evaluations. Results of three experiments provide
   converging evidence in support of the theory.},
