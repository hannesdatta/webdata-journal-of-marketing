   Indiana Univ, Kelley Sch Business, Bloomington, IN 47405 USA.
   Univ Pittsburgh, Katz Sch Business, Pittsburgh, PA 15260 USA.},
DOI = {10.1509/jmkr.41.3.269.35989},
