@article{ WOS:000222514000005,
Author = {Soman, D and Cheema, A},
Title = {When goals are counterproductive: The effects of violation of a
   behavioral goal on subsequent performance},
Journal = {JOURNAL OF CONSUMER RESEARCH},
Year = {2004},
Volume = {31},
Number = {1},
Pages = {52-62},
Month = {JUN},
Abstract = {A considerable body of research supports the idea that individuals who
   set behavioral goals perform better than others who set no goals. In
   this article, we propose that in addition to the positive effects, goals
   may also have a counterproductive effect. Specifically, we propose that
   violating one's goal may cause a deterioration of subsequent performance
   as compared to individuals who have no goals. When the violation of
   one's goal is coded as a failure, it can result in demotivation,
   negative emotion, and consequently a poorer performance. We report two
   experiments that demonstrate the counterproductive effects of goals and
   discuss potential moderators of this effect along with several possible
   process explanations.},
